@using Business_Manager.Helpers
@{
    string itemName = "ClubBudgetSubsidy";
    string itemNameText = "社團經費補助管理";
    string authority = ViewBag.authority;

    ViewBag.Title = itemNameText;
}

@Html.Resource("css",
    @<style>
    </style>
)

<input type="hidden" id="pageAuthority" value="@authority" />

<div class="row">
    <div class="col-12 title-block">
        <h1 class="pt-2 pb-3">@(itemNameText)</h1>
        <div class="unit-block">
            <div class="button-block mb-1">
                <button type="button" class="btn btn-secondary" onclick="Return@(itemName)()"><i class="fas fa-undo"></i>返回</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="modalQuery@(itemName)">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">搜尋</h5>
                <button class="close" data-dismiss="modal" type="button">
                    <span aria-hidden="true"><i class="fas fa-times"></i></span>
                </button>
            </div>
            <div class="modal-body">
                <form class="container" autocomplete="off" id="queryForm@(itemName)">
                    <fieldset>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="cboSubsidyRangeIdQ@(itemName)">補助區間</label>
                            <div class="col-12">
                                <select class="form-control" id="cboSubsidyRangeIdQ@(itemName)" name="cboSubsidyRangeIdQ@(itemName)"></select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="cboApplyIdQ@(itemName)">申請人</label>
                            <div class="col-12">
                                <select class="form-control" id="cboApplyIdQ@(itemName)" name="cboApplyIdQ@(itemName)"></select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="cboPayeeIdQ@(itemName)">領款人</label>
                            <div class="col-12">
                                <select class="form-control" id="cboPayeeIdQ@(itemName)" name="cboPayeeIdQ@(itemName)"></select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label">狀態</label>
                            <div class="col-lg-3 col-md-4 col-6">
                                <label class="control control-solid control-solid-info control--checkbox">
                                    已補助
                                    <input type="checkbox" id="cbxStatusQ@(itemName)Y" name="cbxStatusQ@(itemName)" value="Y" checked />
                                    <span class="control__indicator"></span>
                                </label>
                            </div>
                            <div class="col-lg-3 col-md-4 col-6">
                                <label class="control control-solid control-solid-info control--checkbox">
                                    未補助
                                    <input type="checkbox" id="cbxStatusQ@(itemName)N" name="cbxStatusQ@(itemName)" value="N" checked />
                                    <span class="control__indicator"></span>
                                </label>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label">申請日期 <input type="checkbox" id="cbxUseDate@(itemName)" /></label>
                            <div class="col-6">
                                <input type="text" class="form-control" id="txtStartDateQ@(itemName)" name="txtStartDateQ@(itemName)"
                                       data-msg-extraRegex="請輸入正確的日期格式" data-rule-extraRegex="([12]\d{3}\-(0[1-9]|1[0-2])\-(0[1-9]|[12]\d|3[01]))"
                                       readonly />
                            </div>
                            <div class="col-6">
                                <input type="text" class="form-control" id="txtEndDateQ@(itemName)" name="txtEndDateQ@(itemName)"
                                       data-msg-extraRegex="請輸入正確的日期格式" data-rule-extraRegex="([12]\d{3}\-(0[1-9]|1[0-2])\-(0[1-9]|[12]\d|3[01]))"
                                       readonly />
                            </div>
                        </div>
                    </fieldset>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" onclick="Query@(itemName)()" style="margin: auto;"><i class="fal fa-wrench"></i>套用</button>
            </div>
        </div>
    </div>
</div>

<div class="row" id="listData@(itemName)">
    <div class="col-12">
        <div class="card card-shadow mb-4">
            <div class="card-body">
                <div class="row mb-3 justify-content-between">
                    <div class="col-auto col-sm-6 col-md-6">
                        <a class="btn btn-primary" data-backdrop="static" data-keyboard="false" data-toggle="modal" data-target="#modalQuery@(itemName)" href="javascript:void(0);"><i class="fal fa-search"></i>搜尋</a>
                    </div>
                    <div class="col-auto col-sm-6 col-md-6 text-right">
                        <a class="btn sub-function auth-subsidy-range" href="javascript:PopViewSubsidyRange('Club');"><i class="fas fa-user"></i>補助區間</a>
                        <a class="btn btn-info auth-add" href="javascript:Edit@(itemName)();"><i class="fas fa-plus"></i>新增</a>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                        <div class="table-custom">
                            <table class="table table-bordered table-striped table-sticky table-stack" id="tblData@(itemName)">
                                <thead>
                                    <tr>
                                        <th scope="col" style="min-width: 75px;">#</th>
                                        <th scope="col" style="min-width: 200px;">社團名稱</th>
                                        <th scope="col" style="min-width: 200px;">社團經費補助</th>
                                        <th scope="col" style="min-width: 125px;">金額</th>
                                        <th scope="col" style="min-width: 125px;">申請時間</th>
                                        <th scope="col" style="min-width: 150px;">申請人</th>
                                        <th scope="col" style="min-width: 150px;">領款人</th>
                                        <th class="text-center" scope="col" style="min-width: 350px;">操作</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                        <div class="pagination" id="page@(itemName)"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row" id="editData@(itemName)" style="display: none;">
    <div class="col-12">
        <div class="card card-shadow mb-4">
            <div class="card-body">
                <div class="tab">
                    <ul class="nav nav-tabs nav-fill mb-3" role="tablist">
                        <li class="nav-item">
                            <a class="nav-link active" data-toggle="tab" href="#tabApply">
                                <i class="fas fa-copyright"></i> 補助申請
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#tabFile">
                                <i class="fas fa-upload"></i> 憑證上傳
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#tabDetail">
                                <i class="fas fa-file-invoice-dollar"></i> 經費明細
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" data-toggle="tab" href="#tabParticipants">
                                <i class="fas fa-user-friends"></i> 參與人員
                            </a>
                        </li>
                    </ul>
                </div>
                <div class="tab-content">
                    <div class="tab-pane active" id="tabApply">
                        <div class="card card-shadow">
                            <div class="card-body">
                                <form class="container custom-form-container" autocomplete="off" id="editForm@(itemName)">
                                    <fieldset>
                                        <input type="hidden" id="ClubBudgetSubsidyId" name="ClubBudgetSubsidyId" value="-1" />
                                        <input type="hidden" id="Status" name="Status" value="" />
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="cboClubId@(itemName)">社團 <span class="text-danger">*</span></label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <select class="form-control" id="cboClubId@(itemName)" name="cboClubId@(itemName)"
                                                        data-msg-required="請選擇社團" data-rule-required="true"></select>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="cboSubsidyRangeId@(itemName)">社團經費補助 <span class="text-danger">*</span></label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <select class="form-control" id="cboSubsidyRangeId@(itemName)" name="cboSubsidyRangeId@(itemName)"
                                                        data-msg-required="請選擇社團經費補助" data-rule-required="true"></select>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="txtAmount@(itemName)">金額 <span class="text-danger">*</span></label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <input type="number" class="form-control" id="txtAmount@(itemName)" name="txtAmount@(itemName)" placeholder="請輸入金額"
                                                       data-msg-required="請輸入金額" data-rule-required="true"
                                                       data-msg-min="請輸入金額" min="1"
                                                       value="0" />
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="txtApplyDate@(itemName)">申請時間 <span class="text-danger">*</span></label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <input type="text" class="form-control" id="txtApplyDate@(itemName)" name="txtApplyDate@(itemName)" placeholder="請輸入申請日期"
                                                       data-msg-extraRegex="請輸入正確的日期格式" data-rule-extraRegex="([12]\d{3}\-(0[1-9]|1[0-2])\-(0[1-9]|[12]\d|3[01]))"
                                                       data-msg-required="請輸入申請日期" data-rule-required="true"
                                                       readonly />
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="cboApplyId@(itemName)">申請人 <span class="text-danger">*</span></label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <select class="form-control" id="cboApplyId@(itemName)" name="cboApplyId@(itemName)"
                                                        data-msg-required="請選擇申請人" data-rule-required="true"></select>
                                            </div>
                                        </div>
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="cboPayeeId@(itemName)">領款人 <span class="text-danger">*</span></label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <select class="form-control" id="cboPayeeId@(itemName)" name="cboPayeeId@(itemName)"
                                                        data-msg-required="請選擇領款人" data-rule-required="true"></select>
                                            </div>
                                        </div>
                                    </fieldset>
                                </form>
                            </div>
                            <div class="card-footer text-center text-lg-right read-hide">
                                <button type="button" class="btn btn-success" onclick="Save@(itemName)()"><i class="fas fa-save"></i>儲存</button>
                            </div>
                        </div>
                    </div>
                    <div class="tab-pane" id="tabFile">
                        <input type="hidden" id="RangeType" name="RangeType" value="Club" />
                        @Html.Partial("SubsidyFile")
                    </div>
                    <div class="tab-pane" id="tabDetail">
                        @Html.Partial("ClubBudgetDetail")
                    </div>
                    <div class="tab-pane" id="tabParticipants">
                        @Html.Partial("ClubBudgetParticipant")
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@Html.Partial("ViewSubsidyRange")
@Html.Partial("ViewUser")

@Html.Resource("js",
    @<script>
        let objPage@(itemName) = {
            "pageIndex": 0,
            "pageSize": 10
        };

        let objForm@(itemName) = "#editForm@(itemName)";

        $(document).ready(function () {
            let currentDate = new Date();

            Suites.DatePicker("txtEndDateQ@(itemName)", currentDate);
            currentDate.setDate(currentDate.getDate() - 7);
            Suites.DatePicker("txtStartDateQ@(itemName)", currentDate);

            ComboBoxs.Default("#cboSubsidyRangeIdQ@(itemName)", "@Url.Action("GetSubsidyRange", "ApplyForm")", { "RangeType": "Club", "Status": "A" }, "", "ALL", "", "AnnualWithRangeName", "SubsidyRangeId");
            ComboBoxs.Default("#cboApplyIdQ@(itemName)", "@Url.Action("GetUser", "BasicInformation")", {}, "", "ALL", "", "UserWithDepartment", "UserId");
            ComboBoxs.Default("#cboPayeeIdQ@(itemName)", "@Url.Action("GetUser", "BasicInformation")", {}, "", "ALL", "", "UserWithDepartment", "UserId");

            $("#cboSubsidyRangeIdQ@(itemName), #cboApplyIdQ@(itemName), #cboPayeeIdQ@(itemName)").select2({
                dropdownAutoWidth: true,
                width: "100%"
            });

            Query@(itemName)();

            $("#editData@(itemName) a[data-toggle='tab']").on("shown.bs.tab", function (e) {
                InitParameter@(itemName)($(this));
            });
        });

        function Query@(itemName)() {
            objPage@(itemName).pageIndex = 0;
            InitData@(itemName)(objPage@(itemName));

            $("#modalQuery@(itemName)").modal("hide");
        }

        function InitData@(itemName)(objPage) {
            let innerHtml = '';
            let pageCount = 0;

            let statusList = [];
            $.each($("input[name='cbxStatusQ@(itemName)']:checked"), function () {
                statusList.push($(this).val());
            });

            let targetUrl = "@Url.Action("GetClubBudgetSubsidy", "ApplyForm")";
            let postData = {
                "OrderBy": "",
                "PageIndex": (objPage.pageIndex + 1),
                "PageSize": objPage.pageSize,
                "SubsidyRangeId": $("#cboSubsidyRangeIdQ@(itemName)").val(),
                "ApplyId": $("#cboApplyIdQ@(itemName)").val(),
                "PayeeId": $("#cboPayeeIdQ@(itemName)").val(),
                "StartDate": ($("#cbxUseDate@(itemName)").is(":checked") ? $("#txtStartDateQ@(itemName)").val() : ""),
                "EndDate": ($("#cbxUseDate@(itemName)").is(":checked") ? $("#txtEndDateQ@(itemName)").val() : ""),
                "Status": statusList.join(",")
            };

            let data = DataAccess.Get(targetUrl, postData, false);

            if (data.status == "success") {
                pageCount = data.result.length > 0 ? parseInt(data.result[0]["TotalCount"]) : 0;

                if (pageCount > 0) {
                    objPage = PageCaculate(pageCount, objPage);

                    $.each(data.result, function (i, item) {
                        let _row = (objPage.pageSize * objPage.pageIndex + (i + 1)) + ".";

                        innerHtml += `<tr>
                                        <td><span class="stack-title">#</span>${_row}</td>
                                        <td><span class="stack-title">社團名稱</span>${item.ClubName}</td>
                                        <td>
                                          <span class="stack-title">社團經費補助</span>
                                          ${item.AnnualWithRangeName}
                                          ${item.Status == `Y` ? `<span class="text-success"><i class="fas fa-badge-check"></i></span>` : ``}
                                        </td>
                                        <td><span class="stack-title">金額</span>${item.Amount.toLocaleString(`en`)}</td>
                                        <td><span class="stack-title">申請時間</span>${item.ApplyDate}</td>
                                        <td class="ellipsis" data-toggle="tooltip" title="${item.ApplyNo} ${item.ApplyName}" style="max-width: 150px;"><span class="stack-title">申請人</span><code>${item.ApplyNo}</code> ${item.ApplyName}</td>
                                        <td class="ellipsis" data-toggle="tooltip" title="${item.PayeeNo} ${item.PayeeName}" style="max-width: 150px;"><span class="stack-title">領款人</span><code>${item.PayeeNo}</code> ${item.PayeeName}</td>
                                        <td class="text-center active-content">
                                          <span class="stack-title">操作</span>
                                          ${item.Status == `Y` ? `<a class="active-link auth-read" href="javascript:Read@(itemName)(${item.ClubBudgetSubsidyId});"><i class="fas fa-eye"></i></a>` : ``}
                                          ${item.Status == `N` ? `<a class="active-link auth-update" href="javascript:Edit@(itemName)(${item.ClubBudgetSubsidyId});"><i class="fas fa-edit"></i></a>` : ``}
                                          ${item.Status == `N` ? `<a class="active-link auth-delete" href="javascript:Delete@(itemName)(${item.ClubBudgetSubsidyId})"});"><i class="fas fa-trash-alt"></i></a>` : `<a class="active-link auth-delete active-disable" href="javascript:void(0);"><i class="fas fa-trash-alt"></i></a>`}
                                          <a class="active-link" href="javascript:Word@(itemName)(${item.ClubBudgetSubsidyId});"><i class="fas fa-file-word"></i><span>單據匯出</span></a>
                                          ${item.Status == `N` ? `<a class="active-link auth-check" href="javascript:Check@(itemName)(${item.ClubBudgetSubsidyId})"});"><i class="fas fa-check"></i><span>補助確認</span></a>` : `<a class="active-link auth-redeem active-disable" href="javascript:void(0);"><i class="fas fa-check"></i> 補助確認</a>`}
                                        </td>
                                      </tr>`;
                    });
                } else {
                    innerHtml += `<tr>
                                    <td class="text-center" colspan="8" style="background-color: #fff3cd;">
                                      <i class="fal fa-exclamation-circle"></i>&nbsp;目前尚無資料。
                                    </td>
                                  </tr>`;
                }
            } else {
                alert(data.msg, "alert", 0);
            }

            $("#tblData@(itemName) tbody").html(innerHtml);
            TableComponentInit();
            Pager("#page@(itemName)", pageCount, objPage, InitData@(itemName));
        }

        function Read@(itemName)(ClubBudgetSubsidyId) {
            Edit@(itemName)(ClubBudgetSubsidyId);
            $(objForm@(itemName) + " fieldset").attr("disabled", true);
            $(objForm@(itemName) + " select").attr("disabled", true);
            $(".read-hide").hide();
        }

        function Edit@(itemName)(ClubBudgetSubsidyId) {
            $("#ClubBudgetSubsidyId").val(ClubBudgetSubsidyId ? ClubBudgetSubsidyId : 0);

            $(objForm@(itemName) + " fieldset").attr("disabled", false);
            $(objForm@(itemName) + " select").attr("disabled", false);
            $(".read-hide").show();
            $("#Status").val("N");

            ComboBoxs.Default("#cboClubId@(itemName)", "@Url.Action("GetClub", "Club")", { "Status": "A" }, "", "NA", "", "ClubName", "ClubId");
            ComboBoxs.Default("#cboSubsidyRangeId@(itemName)", "@Url.Action("GetSubsidyRange", "ApplyForm")", { "RangeType": "Club", "Status": "A" }, "", "NA", "", "AnnualWithRangeName", "SubsidyRangeId");
            ComboBoxs.Default("#cboApplyId@(itemName)", "@Url.Action("GetUser", "BasicInformation")", {}, "", "NA", "", "UserWithDepartment", "UserId");
            ComboBoxs.Default("#cboPayeeId@(itemName)", "@Url.Action("GetUser", "BasicInformation")", {}, "", "NA", "", "UserWithDepartment", "UserId");

            let currentDate = new Date();
            Suites.DatePicker("txtApplyDate@(itemName)", currentDate);

            Switch("#editData@(itemName), .unit-block", "#listData@(itemName)");

            if (ClubBudgetSubsidyId > 0) {
                let targetUrl = "@Url.Action("GetClubBudgetSubsidy", "ApplyForm")";
                let postData = {
                    "ClubBudgetSubsidyId": ClubBudgetSubsidyId
                };

                let data = DataAccess.Get(targetUrl, postData, false);

                if (data.status == "success") {
                    $.each(data.result, function (i, item) {
                        $("#cboClubId@(itemName)").val(item.ClubId);
                        $("#cboSubsidyRangeId@(itemName)").val(item.SubsidyRangeId);
                        $("#txtAmount@(itemName)").val(item.Amount);
                        $("#txtApplyDate@(itemName)").val(item.ApplyDate);
                        $("#cboApplyId@(itemName)").val(item.ApplyId);
                        $("#cboPayeeId@(itemName)").val(item.PayeeId);
                        $("#Status").val(item.Status);
                    });
                } else {
                    alert(data.msg, "alert", 0);
                }
            }

            if ($("#editData@(itemName) .nav-tabs > .nav-item > .nav-link").first().hasClass("active")) {
                InitParameter@(itemName)($("#editData@(itemName) .nav-tabs > .nav-item").first().find("a"));
            } else {
                $("#editData@(itemName) .nav-tabs > .nav-item").first().find("a").trigger("click");
            }

            $("#cboClubId@(itemName), #cboSubsidyRangeId@(itemName), #cboApplyId@(itemName), #cboPayeeId@(itemName)").select2({
                dropdownAutoWidth: true,
                width: "100%"
            });
        }

        function Save@(itemName)() {
            if ($(objForm@(itemName)).valid()) {
                let ClubBudgetSubsidyId = parseInt($("#ClubBudgetSubsidyId").val());

                if (ClubBudgetSubsidyId <= 0) {
                    let targetUrl = "@Url.Action("AddClubBudgetSubsidy", "ApplyForm")";
                    let postData = {
                        "ClubId": $("#cboClubId@(itemName)").val(),
                        "SubsidyRangeId": $("#cboSubsidyRangeId@(itemName)").val(),
                        "Amount": $("#txtAmount@(itemName)").val(),
                        "ApplyDate": $("#txtApplyDate@(itemName)").val(),
                        "ApplyId": $("#cboApplyId@(itemName)").val(),
                        "PayeeId": $("#cboPayeeId@(itemName)").val()
                    };

                    let data = DataAccess.EditContinued(targetUrl, postData, false);

                    if (data) {
                        $.each(data.result, function (i, item) {
                            $("#ClubBudgetSubsidyId").val(item.ClubBudgetSubsidyId);
                            $("#Status").val(item.Status);
                        });

                        swal.fire({
                            icon: "success",
                            title: "新增完成"
                        });
                    }
                } else {
                    let targetUrl = "@Url.Action("UpdateClubBudgetSubsidy", "ApplyForm")";
                    let postData = {
                        "ClubBudgetSubsidyId": ClubBudgetSubsidyId,
                        "ClubId": $("#cboClubId@(itemName)").val(),
                        "SubsidyRangeId": $("#cboSubsidyRangeId@(itemName)").val(),
                        "Amount": $("#txtAmount@(itemName)").val(),
                        "ApplyDate": $("#txtApplyDate@(itemName)").val(),
                        "ApplyId": $("#cboApplyId@(itemName)").val(),
                        "PayeeId": $("#cboPayeeId@(itemName)").val()
                    };

                    let result = DataAccess.Update(targetUrl, postData, false, true);
                }
            }
        }

        function Delete@(itemName)(ClubBudgetSubsidyId) {
            swal.fire({
                titleText: "確認要刪除嗎?",
                text: "此動作無法復原，請確認是否要執行!",
                icon: "warning",
                allowOutsideClick: false,
                allowEscapeKey: false,
                showCancelButton: true,
                confirmButtonColor: '#d33',
                reverseButtons: true
            }).then((result) => {
                if (result.isConfirmed) {
                    let targetUrl = "@Url.Action("DeleteClubBudgetSubsidy", "ApplyForm")";
                    let postData = {
                        "ClubBudgetSubsidyId": ClubBudgetSubsidyId
                    };

                    let result = DataAccess.Delete(targetUrl, postData, false, true);

                    if (result) {
                        Query@(itemName)();
                    }
                }
            });
        }

        function Check@(itemName)(ClubBudgetSubsidyId) {
            swal.fire({
                titleText: "確認要通過此社團經費補助嗎?",
                text: "此動作無法復原，請確認是否要執行!",
                icon: "warning",
                allowOutsideClick: false,
                allowEscapeKey: false,
                showCancelButton: true,
                confirmButtonColor: '#d33',
                reverseButtons: true
            }).then((result) => {
                if (result.isConfirmed) {
                    let targetUrl = "@Url.Action("UpdateClubBudgetSubsidyStatus", "ApplyForm")";
                    let postData = {
                        "ClubBudgetSubsidyId": ClubBudgetSubsidyId
                    };

                    let result = DataAccess.Update(targetUrl, postData, false, true);

                    if (result) {
                        Return@(itemName)();
                    }
                }
            });
        }

        function Word@(itemName)(ClubBudgetSubsidyId) {
            let targetUrl = "@Url.Action("ClubBudgetSubsidyWordDownload", "ApplyForm")";
            let postData = {
                "ClubBudgetSubsidyId": ClubBudgetSubsidyId
            };

            FileAccess.AjaxDownload(targetUrl, postData, false);
        }

        function Return@(itemName)() {
            ResetForm(objForm@(itemName));
            Switch("#listData@(itemName)", "#editData@(itemName), .unit-block");
            Query@(itemName)();
        }

        function InitParameter@(itemName)(selector) {
            let initFnString = "Expand";
            let content = selector.attr("href").split('#tab')[1].split('@(itemName)')[0];

            switch (content) {
                case "File":
                    initFnString += "SubsidyFile";
                    break;
                case "Detail":
                    initFnString += "ClubBudgetDetail";
                    break;
                case "Participants":
                    initFnString += "ClubBudgetParticipant";
                    break;
            }

            let initFn = window[initFnString];
            if (initFn && typeof (initFn) == "function") {
                initFn();
            }
        }
    </script>
)