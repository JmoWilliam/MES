@using Business_Manager.Helpers
@{
    string itemName = "ToolModelManagment";
    string itemNameText = "工具型號資料管理";
    string authority = ViewBag.authority;
}

@Html.Resource("css",
    @<style>
    </style>
                                        )

<input type="hidden" id="pageAuthority" value="@authority" />

<div class="row">
    <div class="col-12 title-block">
        <h1 class="pt-2 pb-3">@(itemNameText)</h1>
        <div class="unit-block">
            <div class="button-block mb-1">
                <button type="button" class="btn btn-secondary" onclick="Return@(itemName)()"><i class="fas fa-undo"></i>返回</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="modalQuery@(itemName)">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">搜尋</h5>
                <button class="close" data-dismiss="modal" type="button">
                    <span aria-hidden="true"><i class="fas fa-times"></i></span>
                </button>
            </div>
            <div class="modal-body">
                <form class="container" autocomplete="off" id="queryForm@(itemName)">
                    <fieldset>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="cboToolGroupQ@(itemName)">工具群組 <span class="text-danger">*</span></label>
                            <div class="col-12">
                                <select class="form-control" id="cboToolGroupQ@(itemName)" name="cboToolGroupQ@(itemName)">
                                    <option value="">-- 請選擇工具群組 --</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="cboToolClassQ@(itemName)">工具類別 <span class="text-danger">*</span></label>
                            <div class="col-12">
                                <select class="form-control" id="cboToolClassQ@(itemName)" name="cboToolClassQ@(itemName)">
                                    <option value="">-- 請選擇工具類別 --</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="cboToolCategoryQ@(itemName)">工具種類 <span class="text-danger">*</span></label>
                            <div class="col-12">
                                <select class="form-control" id="cboToolCategoryQ@(itemName)" name="cboToolCategoryQ@(itemName)">
                                    <option value="">-- 請選擇工具種類 --</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="txtToolModelNoQ@(itemName)">工具型號編號</label>
                            <div class="col-12">
                                <input type="text" class="form-control" id="txtToolModelNoQ@(itemName)" name="txtToolModelNoQ@(itemName)" placeholder="請輸入工具型號編號" />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="txtToolModelNameQ@(itemName)">工具型號名稱</label>
                            <div class="col-12">
                                <input type="text" class="form-control" id="txtToolModelNameQ@(itemName)" name="txtToolModelNameQ@(itemName)" placeholder="請輸入工具品名名稱" />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="cboSupplierQ@(itemName)">供應商 <span class="text-danger">*</span></label>
                            <div class="col-12">
                                <select class="form-control" id="cboSupplierQ@(itemName)" name="cboSupplierQ@(itemName)">
                                    <option value="">-- 請選擇供應商 --</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label">狀態</label>
                            <div class="col-lg-3 col-md-4 col-6">
                                <label class="control control-solid control-solid-info control--checkbox">
                                    啟用
                                    <input type="checkbox" id="cbxStatusQ@(itemName)A" name="cbxStatusQ@(itemName)" value="A" checked />
                                    <span class="control__indicator"></span>
                                </label>
                            </div>
                            <div class="col-lg-3 col-md-4 col-6">
                                <label class="control control-solid control-solid-info control--checkbox">
                                    停用
                                    <input type="checkbox" id="cbxStatusQ@(itemName)S" name="cbxStatusQ@(itemName)" value="S" />
                                    <span class="control__indicator"></span>
                                </label>
                            </div>
                        </div>
                    </fieldset>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" onclick="Query@(itemName)()" style="margin: auto;"><i class="fal fa-wrench"></i>套用</button>
            </div>
        </div>
    </div>
</div>

<div class="row" id="listData@(itemName)">
    <div class="col-12">
        <div class="card card-shadow mb-4">
            <div class="card-body">
                <div class="row mb-3">
                    <div class="col-6 col-sm-6 col-md-6">
                        <a class="btn btn-primary" data-backdrop="static" data-keyboard="false" data-toggle="modal" data-target="#modalQuery@(itemName)" href="javascript:void(0);">
                            <i class="fal fa-search"></i>搜尋
                        </a>
                    </div>
                    <div class="col-6 col-sm-6 col-md-6 text-right">
                        <a class="btn btn-info auth-add" href="javascript:Edit@(itemName)();"><i class="fas fa-plus"></i>新增</a>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                        <div class="table-custom">
                            <table class="table table-bordered table-striped table-sticky" id="tblData@(itemName)">
                                <thead>
                                    <tr>
                                        <th scope="col" style="min-width: 75px;">#</th>
                                        <th scope="col" style="min-width: 150px;">工具群組名稱</th>
                                        <th scope="col" style="min-width: 150px;">工具類別名稱</th>
                                        <th scope="col" style="min-width: 150px;">工具種類名稱</th>
                                        <th scope="col" style="min-width: 150px;">工具型號名稱</th>
                                        <th scope="col" style="min-width: 150px;">工具型號編號</th>
                                        <th scope="col" style="min-width: 150px;">供應商</th>
                                        <th class="text-center" scope="col" style="min-width: 125px;">狀態</th>
                                        <th class="text-center" scope="col" style="min-width: 265px;">操作</th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                        <div class="pagination" id="page@(itemName)"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row" id="editData@(itemName)" style="display: none;">
    <div class="col-12">
        <div class="card card-shadow mb-4">
            <div class="card-body">
                <form class="container" autocomplete="off" id="editForm@(itemName)">
                    <fieldset>
                        <input type="hidden" id="ToolModelId" name="ToolModelId" value="-1" />
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="cboToolGroup@(itemName)">工具群組 <span class="text-danger">*</span></label>
                            <div class="col-12">
                                <select class="form-control" id="cboToolGroup@(itemName)" name="cboToolGroup@(itemName)">
                                    <option value="">-- 請選擇工具群組 --</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="cboToolClass@(itemName)">工具類別 <span class="text-danger">*</span></label>
                            <div class="col-12">
                                <select class="form-control" id="cboToolClass@(itemName)" name="cboToolClass@(itemName)">
                                    <option value="">-- 請選擇工具類別 --</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="cboToolCategory@(itemName)">工具種類 <span class="text-danger">*</span></label>
                            <div class="col-12">
                                <select class="form-control" id="cboToolCategory@(itemName)" name="cboToolCategory@(itemName)"  data-rule-required="true">
                                    <option value="">-- 請選擇工具種類 --</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="cboSupplier@(itemName)">供應商 <span class="text-danger">*</span></label>
                            <div class="col-12">
                                <select class="form-control" id="cboSupplier@(itemName)" name="cboSupplier@(itemName)"  data-rule-required="true">
                                    <option value="">-- 請選擇供應商 --</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="txtToolModelNo@(itemName)">工具型號編號 <span class="text-danger">*</span></label>
                            <div class="col-12">
                                <input type="text" class="form-control" id="txtToolModelNo@(itemName)" name="txtToolModelNo@(itemName)" placeholder="請輸入工具型號編號"
                                       data-msg-required="請輸入工具型號編號" data-rule-required="true"
                                       data-msg-maxlength="必須少於 50 個字元" maxlength="50" />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="txtToolModelName@(itemName)">工具型號名稱 <span class="text-danger">*</span></label>
                            <div class="col-12">
                                <input type="text" class="form-control" id="txtToolModelName@(itemName)" name="txtToolModelName@(itemName)" placeholder="請輸入工具型號名稱"
                                       data-msg-required="請輸入工具型號名稱" data-rule-required="true"
                                       data-msg-maxlength="必須少於 50 個字元" maxlength="50" />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="txtToolModelErpNo@(itemName)">ERP型號 </label>
                            <div class="col-12">
                                <input type="text" class="form-control" id="txtToolModelErpNo@(itemName)" name="txtToolModelErpNo@(itemName)" placeholder="請輸入ERP型號"
                                       data-msg-required="請輸入ERP型號"
                                       data-msg-maxlength="必須少於 50 個字元" maxlength="50" />
                            </div>
                        </div>

                    </fieldset>
                </form>
            </div>
            <div class="card-footer text-center text-lg-right">
                <button type="button" class="btn btn-success" onclick="Save@(itemName)()"><i class="fas fa-save"></i>儲存</button>
            </div>
        </div>
    </div>
</div>

<div class="advanced-block">
    <div class="row">
        <div class="col-12">
            <ul class="nav flag-tabs nav-fill" role="tablist">
                <li class="flag-item">
                    <a class="flag-link active" data-toggle="tab" href="#tabToolModelSpec">
                        <i class="fas fa-garage-car"></i> 工具型號規格
                    </a>
                </li>
            </ul>
        </div>
    </div>
    <div class="tab-content">
        <div class="tab-pane active" id="tabToolModelSpec">
            @Html.Partial("ToolModelSpec")
        </div>
    </div>
</div>

@Html.Partial("ViewToolModelBatch")

@Html.Resource("js",
    @<script>
        let objPage@(itemName) = {
            "pageIndex": 0,
            "pageSize": 10
        };
        let objForm@(itemName) = "#editForm@(itemName)";

        $(document).ready(function () {           

            ComboBoxs.Default("#cboToolGroupQ@(itemName)", "@Url.Action("GetToolGroup", "Tool")", "", "", "NA", "", "ToolGroupName", "ToolGroupId");
            ComboBoxs.Default("#cboToolClassQ@(itemName)", "@Url.Action("GetToolClass", "Tool")", "", "", "NA", "", "ToolClassName", "ToolClassId");
            ComboBoxs.Default("#cboToolCategoryQ@(itemName)", "@Url.Action("GetToolCategory", "Tool")", "", "", "NA", "", "ToolCategoryName", "ToolCategoryId");
            ComboBoxs.Default("#cboSupplierQ@(itemName)", "@Url.Action("GetSupplier", "ScmBasicInformation")", "", "", "NA", "", "SupplierName", "SupplierId");

            $("#cboToolGroupQ@(itemName)").change(function () {
                ComboBoxs.Default("#cboToolClassQ@(itemName)", "@Url.Action("GetToolClass", "Tool")", { "ToolGroupId": $("#cboToolGroupQ@(itemName)").val(), "Status": "A"}, "", "NA", "", "ToolClassName", "ToolClassId");
            });

            $("#cboToolClassQ@(itemName)").change(function () {
                ComboBoxs.Default("#cboToolCategoryQ@(itemName)", "@Url.Action("GetToolCategory", "Tool")", { "ToolClassId": $("#cboToolClassQ@(itemName)").val(), "Status": "A" }, "", "NA", "", "ToolCategoryName", "ToolCategoryId");
            });



            //下拉選單樣式
            if (!isMobile) {
                $(`#cboToolGroupQ@(itemName), #cboToolClassQ@(itemName), #cboToolCategoryQ@(itemName), #cboSupplierQ@(itemName)`).select2
                ({
                    dropdownAutoWidth: true,
                    width: "100%"
                });
            }

            Query@(itemName)();
        });

        function Query@(itemName)() {
            objPage@(itemName).pageIndex = 0;
            InitData@(itemName)(objPage@(itemName));

            $("#modalQuery@(itemName)").modal("hide");
        }

        function InitData@(itemName)(objPage) {
            let innerHtml = '';
            let pageCount = 0;


            let statusList = [];
            $.each($("input[name='cbxStatusQ@(itemName)']:checked"), function () {
                statusList.push($(this).val());
            });

            let targetUrl = "@Url.Action("GetToolModel", "Tool")";
            let postData = {
                "OrderBy": "",
                "PageIndex": (objPage.pageIndex + 1),
                "PageSize": objPage.pageSize,
                "ToolModelNo": $("#txtToolModelNoQ@(itemName)").val().trim(),
                "ToolModelName": $("#txtToolModelNameQ@(itemName)").val().trim(),
                "ToolGroupId": $("#cboToolGroupQ@(itemName)").val(),
                "ToolClassId": $("#cboToolClassQ@(itemName)").val(),
                "ToolCategoryId": $("#cboToolCategoryQ@(itemName)").val(),
                "SupplierId": $("#cboSupplierQ@(itemName)").val(),
                "Status": statusList.join(",")
            };

            let data = DataAccess.Get(targetUrl, postData, false);

            if (data.status == "success") {
                pageCount = data.result.length > 0 ? parseInt(data.result[0]["TotalCount"]) : 0;

                if (pageCount > 0) {
                    objPage = PageCaculate(pageCount, objPage);

                    $.each(data.result, function (i, item) {
                        let _row = (objPage.pageSize * objPage.pageIndex + (i + 1)) + ".";

                        innerHtml += `<tr>
                                        <td>${_row}</td>
                                        <td class="ellipsis" data-toggle="tooltip" title="${item.ToolGroupName}" style="max-width: 150px;">${item.ToolGroupName}</td>
                                        <td class="ellipsis" data-toggle="tooltip" title="${item.ToolClassName}" style="max-width: 150px;">${item.ToolClassName}</td>
                                        <td class="ellipsis" data-toggle="tooltip" title="${item.ToolCategoryName}" style="max-width: 150px;">${item.ToolCategoryName}</td>
                                        <td class="ellipsis" data-toggle="tooltip" title="${item.ToolModelName}" style="max-width: 150px;">${item.ToolModelName}</td>
                                        <td class="ellipsis" data-toggle="tooltip" title="${item.ToolModelNo}" style="max-width: 150px;">${item.ToolModelNo}</td>
                                        <td class="ellipsis" data-toggle="tooltip" title="${item.SupplierName}" style="max-width: 150px;">${item.SupplierName != null ? item.SupplierName : `未設定`}</td>
                                        <td class="text-center auth-status-switch">
                                          <input type="checkbox" name="chkStatus@(itemName)" data-toolmodel-id="${item.ToolModelId}" ${item.Status == `A` ? `checked` : ``} />
                                        </td>
                                        <td class="text-center un-auth auth-status-switch">${StatusName(`Status`, item.Status)}</td>
                                        <td class="text-center active-content">
                                          <a class="active-link auth-update" href="javascript:Edit@(itemName)(${item.ToolModelId});"><i class="fas fa-edit"></i></a>
                                          <a class="active-link auth-delete" href="javascript:Delete@(itemName)(${item.ToolModelId});"><i class="fas fa-trash-alt"></i></a>
                                          <a class="active-link auth-delete" href="javascript:PopViewToolModelBatch(${item.ToolModelId},\'${item.ToolModelName}\');"><i class="fas fa-shopping-cart"></i></a>
                                        </td>
                                      </tr>`;
                    });
                } else {
                    innerHtml += `<tr>
                                    <td class="text-center" colspan="9" style="background-color: #fff3cd;">
                                      <i class="fal fa-exclamation-circle"></i>&nbsp;目前尚無資料。
                                    </td>
                                  </tr>`;
                }
            } else {
                alert(data.msg, "alert", 0);
            }

            $("#tblData@(itemName) tbody").html(innerHtml);
            TableComponentInit();
            Pager("#page@(itemName)", pageCount, objPage, InitData@(itemName));

            $("input[name='chkStatus@(itemName)']").bootstrapToggle({
                on: "啟用",
                off: "停用",
                onstyle: "success",
                offstyle: "danger",
                size: "mini"
            });

            $("input[name='chkStatus@(itemName)'][data-toolmodel-id]").change(StatusSwitch@(itemName));
        }

        function Edit@(itemName)(ToolModelId) {
            $("#ToolModelId").val(ToolModelId ? ToolModelId : 0);
            $("#txtToolModelNo@(itemName)").attr("readonly", false);

            Switch("#editData@(itemName), .unit-block", "#listData@(itemName)");
            ComboBoxs.Default("#cboToolGroup@(itemName)", "@Url.Action("GetToolGroup", "Tool")", "", "", "NA", "", "ToolGroupName", "ToolGroupId");
            ComboBoxs.Default("#cboToolClass@(itemName)", "@Url.Action("GetToolClass", "Tool")", "", "", "NA", "", "ToolClassName", "ToolClassId");
            ComboBoxs.Default("#cboToolCategory@(itemName)", "@Url.Action("GetToolCategory", "Tool")", "", "", "NA", "", "ToolCategoryName", "ToolCategoryId");
            ComboBoxs.Default("#cboSupplier@(itemName)", "@Url.Action("GetSupplier", "ScmBasicInformation")", "", "", "NA", "", "SupplierName", "SupplierId");           

            var UserNo="@Session["UserNo"]";
            if (UserNo == "ZY00076") {
                $("#cboToolGroup@(itemName)").val(1);
                $("#cboToolGroup@(itemName)").trigger("change");
                $("#cboToolGroup@(itemName)").change(function () {
                    ComboBoxs.Default("#cboToolClass@(itemName)", "@Url.Action("GetToolClass", "Tool")", { "ToolGroupId": $("#cboToolGroup@(itemName)").val(), "Status": "A"}, "", "NA", "", "ToolClassName", "ToolClassId");
                });

                $("#cboToolClass@(itemName)").val(2);
                $("#cboToolClass@(itemName)").trigger("change");
                $("#cboToolClass@(itemName)").change(function () {
                    ComboBoxs.Default("#cboToolCategory@(itemName)", "@Url.Action("GetToolCategory", "Tool")", { "ToolClassId": $("#cboToolClass@(itemName)").val(), "Status": "A" }, "", "NA", "", "ToolCategoryName", "ToolCategoryId");
                });
            } else {
                $("#cboToolGroup@(itemName)").change(function () {
                    ComboBoxs.Default("#cboToolClass@(itemName)", "@Url.Action("GetToolClass", "Tool")", { "ToolGroupId": $("#cboToolGroup@(itemName)").val(), "Status": "A"}, "", "NA", "", "ToolClassName", "ToolClassId");
                });

                $("#cboToolClass@(itemName)").change(function () {
                    ComboBoxs.Default("#cboToolCategory@(itemName)", "@Url.Action("GetToolCategory", "Tool")", { "ToolClassId": $("#cboToolClass@(itemName)").val(), "Status": "A" }, "", "NA", "", "ToolCategoryName", "ToolCategoryId");
                });
            }

            $("#cboToolCategory@(itemName)").change(function () {
                GetToolMap()
            });

            if (ToolModelId > 0) {
                $("#txtToolModelNo@(itemName)").attr("readonly", true);
                let targetUrl = "@Url.Action("GetToolModel", "Tool")";
                let postData = {
                    "ToolModelId": ToolModelId
                };
                let data = DataAccess.Get(targetUrl, postData, false);
                if (data.status == "success") {
                    $.each(data.result, function (i, item) {
                        $("#cboToolGroup@(itemName)").val(item.ToolGroupId);
                        $("#cboToolClass@(itemName)").val(item.ToolClassId);
                        $("#cboToolCategory@(itemName)").val(item.ToolCategoryId);
                        $("#cboSupplier@(itemName)").val(item.SupplierId);
                        $("#txtToolModelNo@(itemName)").val(item.ToolModelNo);
                        $("#txtToolModelName@(itemName)").val(item.ToolModelName);
                        $("#txtToolModelErpNo@(itemName)").val(item.ToolModelErpNo);
                    });
                    ExpandToolModelSpec();
                } else {
                    alert(data.msg, "alert", 0);
                }
            } else {

            }

            //下拉選單樣式
            if (!isMobile) {
                $(`#cboToolGroup@(itemName), #cboToolClass@(itemName), #cboToolCategory@(itemName), #cboSupplier@(itemName)`).select2
                ({
                    dropdownAutoWidth: true,
                    width: "100%"
                });
            }
        }

        function Save@(itemName)() {
            if ($(objForm@(itemName)).valid()) {
                let ToolModelId = parseInt($("#ToolModelId").val());

                if (ToolModelId <= 0) {
                    let targetUrl = "@Url.Action("AddToolModel", "Tool")";
                    let postData = {
                        "ToolCategoryId": $("#cboToolCategory@(itemName)").val(),
                        "SupplierId": $("#cboSupplier@(itemName)").val(),
                        "ToolModelNo": $("#txtToolModelNo@(itemName)").val().trim(),
                        "ToolModelName": $("#txtToolModelName@(itemName)").val().trim(),
                        "ToolModelErpNo": $("#txtToolModelErpNo@(itemName)").val().trim()
                    };

                    let data = DataAccess.EditContinued(targetUrl, postData, false);
                    if (data) {
                        $.each(data.result, function (i, item) {
                            $("#ToolModelId").val(item.ToolModelId)
                            if (item.ToolModelId > 0) {
                                $("#txtToolModelNo@(itemName)").attr("readonly", true);
                            }
                        });
                        ExpandToolModelSpec();

                        //Return@(itemName)();
                    }
                } else {
                    let targetUrl = "@Url.Action("UpdateToolModel", "Tool")";
                    let postData = {
                        "ToolModelId": ToolModelId,
                        "ToolCategoryId": $("#cboToolCategory@(itemName)").val(),
                        "SupplierId": $("#cboSupplier@(itemName)").val(),
                        "ToolModelName": $("#txtToolModelName@(itemName)").val().trim(),
                        "ToolModelErpNo": $("#txtToolModelErpNo@(itemName)").val().trim()
                    };

                    let result = DataAccess.Update(targetUrl, postData, false, true);

                    if (result) {
                        ExpandToolModelSpec();

                        //Return@(itemName)();
                    }
                }
            }
        }

        function Delete@(itemName)(ToolModelId) {
            swal.fire({
                title: "確認要刪除嗎?",
                text: "此動作無法復原，請確認是否要執行!",
                icon: "warning",
                allowOutsideClick: false,
                allowEscapeKey: false,
                showCancelButton: true,
                confirmButtonColor: '#d33',
                reverseButtons: true
            }).then((result) => {
                if (result.isConfirmed) {
                    let targetUrl = "@Url.Action("DeleteToolModel", "Tool")";
                    let postData = {
                        "ToolModelId": ToolModelId
                    };

                    let result = DataAccess.Delete(targetUrl, postData, false, true);

                    if (result) {
                        Query@(itemName)();
                    }
                }
            });
        }

        function Return@(itemName)() {
            ResetForm(objForm@(itemName));
            Switch("#listData@(itemName)", "#editData@(itemName), .unit-block, .advanced-block");
            Query@(itemName)();
        }

        function StatusSwitch@(itemName)() {
            var toolModelId = $(this).data("toolmodel-id");

            let targetUrl = "@Url.Action("UpdateToolModelStatus", "Tool")";
            let postData = {
                "ToolModelId": toolModelId
            };

            let result = DataAccess.Update(targetUrl, postData, false, true);

            if (result) {
                Return@(itemName)();
            }
        }

        function GetToolMap() {
            let targetUrl = "@Url.Action("GetToolCategory", "Tool")";
            let postData = {
                "ToolCategoryId": $("#cboToolCategory@(itemName)").val(),
            };

            let data = DataAccess.Get(targetUrl, postData, false);

            if (data.status == "success") {
                $.each(data.result, function (i, item) {
                    ComboBoxs.Default("#cboToolGroup@(itemName)", "@Url.Action("GetToolGroup", "Tool")", "", "", "NA", "", "ToolGroupName", "ToolGroupId");
                    ComboBoxs.Default("#cboToolClass@(itemName)", "@Url.Action("GetToolClass", "Tool")", { "ToolGroupId": item.ToolGroupId, "Status": "A" }, "", "NA", "", "ToolClassName", "ToolClassId");
                    ComboBoxs.Default("#cboToolCategory@(itemName)", "@Url.Action("GetToolCategory", "Tool")", { "ToolClassId": item.ToolClassId, "Status": "A" }, "", "NA", "", "ToolCategoryName", "ToolCategoryId");

                    $("#cboToolGroup@(itemName)").val(item.ToolGroupId);
                    $("#cboToolClass@(itemName)").val(item.ToolClassId);
                    $("#cboToolCategory@(itemName)").val(item.ToolCategoryId);
                });
            } else {
                alert(data.msg, "alert", 0);
            }
        }
    </script>
