@using Business_Manager.Helpers
@{
    string itemName = "Interfacing";
    string itemNameText = "系統介接設定";
    string authority = ViewBag.authority;

    ViewBag.Title = itemNameText;
}

@Html.Resource("css",
    @<style>
        .data-selected td {
            font-weight: bold;
            color: #ff0000;
        }
    </style>
)

<input type="hidden" id="pageAuthority" value="@authority" />
 
<div class="row">
    <div class="col-12 title-block">
        <h1 class="pt-2 pb-3">@(itemNameText)</h1>
    </div>
</div>

<div class="modal fade" id="modalQuery@(itemName)">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">搜尋</h5>
                <button class="close" data-dismiss="modal" type="button">
                    <span aria-hidden="true"><i class="fas fa-times"></i></span>
                </button>
            </div>
            <div class="modal-body">
                <form class="container" autocomplete="off" id="queryForm@(itemName)">
                    <fieldset>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="cboCompanyIdQ@(itemName)">公司別</label>
                            <div class="col-12">
                                <select class="form-control" id="cboCompanyIdQ@(itemName)" name="cboCompanyIdQ@(itemName)"></select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="cboInterfacingTypeQ@(itemName)">介接類型</label>
                            <div class="col-12">
                                <select class="form-control" id="cboInterfacingTypeQ@(itemName)" name="cboInterfacingTypeQ@(itemName)"></select>
                            </div>
                        </div>
                    </fieldset>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" onclick="Query@(itemName)()" style="margin: auto;"><i class="fal fa-wrench"></i>套用</button>
            </div>
        </div>
    </div>
</div>

<div class="row" id="listData@(itemName)">
    <div class="col-12">
        <div class="card card-shadow mb-4">
            <div class="card-body">
                <div class="row mb-3">
                    <div class="col-6 col-sm-6 col-md-6">
                        <a class="btn btn-primary" data-backdrop="static" data-keyboard="false" data-toggle="modal" data-target="#modalQuery@(itemName)" href="javascript:void(0);">
                            <i class="fal fa-search"></i>搜尋
                        </a>
                    </div>
                    <div class="col-6 col-sm-6 col-md-6 text-right">
                        <a class="btn btn-info auth-add" href="javascript:Edit@(itemName)();"><i class="fas fa-plus"></i>新增</a>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                        <div class="table-custom">
                            <table class="table table-bordered table-striped table-sticky tiny-table" id="tblData@(itemName)">
                                <thead>
                                    <tr>
                                        <th scope="col" style="min-width: 75px;">#</th>
                                        <th scope="col" style="min-width: 150px;">公司別</th>
                                        <th scope="col" style="min-width: 100px;">介接類型</th>
                                        <th class="text-center" scope="col" style="min-width: 75px;">操作</th>
                                        <th style="padding: 0; min-width: 700px;">
                                            <table class="table table-bordered table-sm mb-0 table-non-rwd table-striped-custom table-row-detail">
                                                <thead>
                                                    <tr>
                                                        <th style="width: 100px;">啟用</th>
                                                        <th style="width: 150px;">連線主機</th>
                                                        <th style="width: 150px;">連線帳號</th>
                                                        <th style="width: 150px;">連線密碼</th>
                                                        <th style="width: 150px;">連線資料庫</th>
                                                    </tr>
                                                </thead>
                                            </table>
                                        </th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                        <div class="pagination" id="page@(itemName)"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="side-nav hs-display" id="editData@(itemName)">
    <div class="container">
        <div class="toolbar">
            <div class="tool-list">
                <a class="set" href="javascript:void(0);" onclick="Operations.Previous()" title="上一筆"><i class="fas fa-chevron-up"></i></a>
                <a class="set" href="javascript:void(0);" onclick="Operations.Next()" title="下一筆"><i class="fas fa-chevron-down"></i></a>
            </div>
            <div class="tool-box">
                <div class="set-area">
                    <a class="set" href="javascript:void(0);" onclick="Operations.Save()" title="儲存"><i class="fas fa-save"></i></a>
                    <a class="set auth-delete" href="javascript:void(0);" onclick="Operations.Delete(0)" title="刪除"><i class="fas fa-trash-alt"></i></a>
                </div>
                <div class="tool-list">
                    <a class="set set-close" href="javascript:void(0);" onclick="Operations.EditClose()" title="關閉"><i class="fas fa-times"></i></a>
                </div>
            </div>
        </div>
        <div class="side-nav-header">
            <div class="nav-title">
                <h1>介接設定資料</h1>
            </div>
        </div>
        <div class="nav-content">
            <div class="main-content">
                <form class="nav-form" autocomplete="off" id="editForm@(itemName)">
                    <fieldset>
                        <input type="hidden" id="InterfacingId" name="InterfacingId" value="-1" />
                        <input type="hidden" id="PreviousId" name="PreviousId" value="-1" />
                        <input type="hidden" id="NextId" name="NextId" value="-1" />
                        <div class="form-group row">
                            <label class="col-12 col-md-3 col-lg-2 col-form-label" for="cboCompanyId@(itemName)">所屬公司 <span class="text-danger">*</span></label>
                            <div class="col-12 col-md-9 col-lg-10">
                                <select class="form-control" id="cboCompanyId@(itemName)" name="cboCompanyId@(itemName)"
                                        data-msg-required="請選擇所屬公司" data-rule-required="true"></select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-md-3 col-lg-2 col-form-label" for="cboInterfacingType@(itemName)">介接類型 <span class="text-danger">*</span></label>
                            <div class="col-12 col-md-9 col-lg-10">
                                <select class="form-control" id="cboInterfacingType@(itemName)" name="cboInterfacingType@(itemName)"
                                        data-msg-required="請選擇介接類型" data-rule-required="true"></select>
                            </div>
                        </div>
                    </fieldset>
                </form>
            </div>

            <div class="detail-content">
                <div class="datail-title">
                    <h2>介接連線</h2>
                    <a class="link" href="javascript:void(0);" onclick="Operations.AddDetail()" title="新增連線"><i class="far fa-plus-circle"></i>新增連線</a>
                </div>
            </div>
        </div>
    </div>
</div>

@Html.Resource("js",
    @<script>
        let objPage@(itemName) = {
            "pageIndex": 0,
            "pageSize": 10
        };
        let objForm@(itemName) = "#editForm@(itemName)";
        let json@(itemName) = {
            CompanyId: -1,
            InterfacingType: "",
            Details: []
        };

        let Operations = new OperationControl@(itemName)();

        $(document).ready(function () {
            ComboBoxs.Default("#cboCompanyIdQ@(itemName)", "@Url.Action("GetCompany", "BasicInformation")", { "Status": "A" }, "", "ALL", "", "CompanyWithNo", "CompanyId");
            ComboBoxs.Default("#cboInterfacingTypeQ@(itemName)", "@Url.Action("GetType", "BasicInformation")", { "TypeSchema": "Interfacing.InterfacingType" }, "", "CHOOSE", "全部", "TypeName", "TypeNo");

            $("#cboCompanyIdQ@(itemName), #cboInterfacingTypeQ@(itemName)").select2({
                dropdownAutoWidth: true,
                width: "100%"
            });

            Query@(itemName)();
        });

        function Query@(itemName)() {
            objPage@(itemName).pageIndex = 0;
            InitData@(itemName)(objPage@(itemName));

            $("#modalQuery@(itemName)").modal("hide");
        }

        function InitData@(itemName)(objPage) {
            let innerHtml = '';
            let pageCount = 0

            let targetUrl = "@Url.Action("GetInterfacing", "SystemInterfacing")";
            let postData = {
                "OrderBy": "",
                "PageIndex": (objPage.pageIndex + 1),
                "PageSize": objPage.pageSize,
                "CompanyId": $("#cboCompanyIdQ@(itemName)").val(),
                "InterfacingType": $("#cboInterfacingTypeQ@(itemName)").val()
            };
            let data = DataAccess.Get(targetUrl, postData, false);

            if (data.status == "success") {
                pageCount = data.result.length > 0 ? parseInt(data.result[0]["TotalCount"]) : 0;

                if (pageCount > 0) {
                    objPage = PageCaculate(pageCount, objPage);

                    $.each(data.result, function (i, item) {
                        let _row = (objPage.pageSize * objPage.pageIndex + (i + 1)) + ".";

                        innerHtml += `<tr data-interfacing-id="${item.InterfacingId}" data-previous="${item.PreviousId}" data-next="${item.NextId}">
                                        <td>${_row}</td>
                                        <td>${(item.LogoIcon == -1 ? `` : `<img class="company-logo" src="/Web/GetFile?fileId=${item.LogoIcon}" />`)}${item.CompanyName}</td>
                                        <td>${item.InterfacingTypeName}</td>
                                        <td class="text-center active-content">
                                          <div class="btn-group mb-4 show" style="margin-bottom: 0 !important; position: static;">
                                            <button type="button" class="btn btn-primary dropdown-toggle" data-interfacing-id="${item.InterfacingId}" style="padding-top: 0; padding-bottom: 0;">動作</button>
                                            <div class="dropdown-menu btn-text-left-block" data-interfacing-id="${item.InterfacingId}" style="overflow-y: auto; z-index: 999; max-height: 150px;">
                                              <a class="dropdown-item btn-text-left auth-update" href="javascript:Edit@(itemName)(${item.InterfacingId});" ><i class="fas fa-edit"></i>修改</a>
                                              <a class="dropdown-item btn-text-left auth-delete" href="javascript:Operations.Delete(${item.InterfacingId});" ><i class="fas fa-trash-alt"></i>刪除</a>
                                            </div>
                                          </div>
                                        </td>
                                        <td style="padding: 0;">
                                          <table class="table table-bordered table-sm mb-0 table-non-rwd table-striped-custom table-row-detail">
                                            <tbody>
                                              ${DetailData@(itemName)(item.InterfacingConnection)}
                                            </tbody>
                                          </table>
                                        </td>
                                      </tr>`;
                    });
                } else {
                    innerHtml += `<tr>
                                    <td class="text-center" colspan="5" style="background-color: #fff3cd;">
                                      <i class="fal fa-exclamation-circle"></i>&nbsp;目前尚無資料。
                                    </td>
                                  </tr>`;
                }
            } else {
                alert(data.msg, "alert", 0);
            }

            $("#tblData@(itemName) tbody").html(innerHtml);
            TableComponentInit();
            Pager("#page@(itemName)", pageCount, objPage, InitData@(itemName));

            $("input[name='chkStatus@(itemName)']").bootstrapToggle({
                on: "啟用",
                off: "停用",
                onstyle: "success",
                offstyle: "danger",
                size: "mini"
            });

            $("input[name='chkStatus@(itemName)'][data-connection-id]").change(function () {
                Operations.StatusSwitch($(this).data("connection-id"));
            });

            $(".dropdown-toggle").off("click").on("click", function () {
                let interfacingId = $(this).data("interfacing-id");

                if ($(`.dropdown-menu[data-interfacing-id='${interfacingId}']`).hasClass("show")) {
                    $(".dropdown-menu").removeClass("show");
                } else {
                    $(".dropdown-menu").removeClass("show");

                    $(`.dropdown-menu[data-interfacing-id='${interfacingId}']`).addClass("show");
                    $(`.dropdown-menu[data-interfacing-id='${interfacingId}']`).css({
                        top: `${$(this).position().top + $(this).height() + 5}px`,
                        left: `${$(this).position().left - $(this).width() + 38}px`
                    });
                }
            });

            $("body").off("click").on("click", function (e) {
                if ($(".dropdown-menu").hasClass("show")) {
                    if (!$(e.target).hasClass("dropdown-toggle")
                        && !$(e.target).parents().hasClass("dropdown-toggle")) {
                        $(".dropdown-menu").removeClass("show");
                    }
                }
            });
        }

        function DetailData@(itemName)(source) {
            let html = '';

            if (source) {
                let json = JSON.parse(source);

                for (let x = 0; x < json.data.length; x++) {
                    html += `<tr>
                               <td style="width: 100px; padding: 0;">
                                 <input type="checkbox" name='chkStatus@(itemName)' data-connection-id="${json.data[x].ConnectionId}" ${(json.data[x].Status == `A` ? `checked` : ``)} />
                               </td>
                               <td style="width: 150px;">${json.data[x].ConnectionServer}</td>
                               <td style="width: 150px;">${json.data[x].ConnectionAccount}</td>
                               <td style="width: 150px;">${json.data[x].ConnectionPassword}</td>
                               <td style="width: 150px;">${json.data[x].ConnectionDatabase}</td>
                             </tr>`;
                }
            }

            return html;
        }

        function Edit@(itemName)(InterfacingId) {
            $("tr[data-interfacing-id]").removeClass("data-selected");
            $("#editData@(itemName) .datail-box").remove();

            json@(itemName) = {
                CompanyId: -1,
                InterfacingType: "",
                Details: []
            };

            $("#InterfacingId").val(InterfacingId ? InterfacingId : 0);
            $("#PreviousId").val(0);
            $("#NextId").val(0);

            $(objForm@(itemName) + " fieldset").attr("disabled", false);
            $(objForm@(itemName) + " select").attr("disabled", false);

            $("#editData@(itemName)").addClass("active");

            ComboBoxs.Default("#cboCompanyId@(itemName)", "@Url.Action("GetCompany", "BasicInformation")", { "Status": "A" }, "", "NA", "", "CompanyWithNo", "CompanyId");
            ComboBoxs.Default("#cboInterfacingType@(itemName)", "@Url.Action("GetType", "BasicInformation")", { "TypeSchema": "Interfacing.InterfacingType" }, "", "NA", "", "TypeName", "TypeNo");
            
            $("#editForm@(itemName) .form-control").off("change").on("change", function () {
                json@(itemName).CompanyId = $("#cboCompanyId@(itemName)").val() ? $("#cboCompanyId@(itemName)").val() : -1;
                json@(itemName).InterfacingType = $("#cboInterfacingType@(itemName)").val() ? $("#cboInterfacingType@(itemName)").val() : -1;
            });

            if (InterfacingId > 0) {
                let targetUrl = "@Url.Action("GetInterfacing", "SystemInterfacing")";
                let postData = {
                    "InterfacingId": InterfacingId
                };
                let data = DataAccess.Get(targetUrl, postData, false);

                if (data.status == "success") {
                    $.each(data.result, function (i, item) {
                        $("#PreviousId").val($(`tr[data-interfacing-id="${InterfacingId}"]`).data("previous"));
                        $("#NextId").val($(`tr[data-interfacing-id="${InterfacingId}"]`).data("next"));

                        $("#cboCompanyId@(itemName)").val(item.CompanyId);
                        $("#cboInterfacingType@(itemName)").val(item.InterfacingType);

                        json@(itemName).CompanyId = item.CompanyId;
                        json@(itemName).InterfacingType = item.InterfacingType;
                        json@(itemName).Details = JSON.parse(item.InterfacingConnection).data;
                    });

                    Operations.InitDetail();
                    $(`tr[data-interfacing-id="${InterfacingId}"]`).addClass("data-selected");
                } else {
                    alert(data.msg, "alert", 0);
                }
            }
        }

        function OperationControl@(itemName)() {
            addMethod(this, "StatusSwitch", function (ConnectionId) {
                let targetUrl = "@Url.Action("UpdateInterfacingConnectionStatus", "SystemInterfacing")";
                let postData = {
                    "ConnectionId": ConnectionId
                };

                let result = DataAccess.Update(targetUrl, postData, false, true);

                if (result) {
                    Query@(itemName)();
                }
            });

            addMethod(this, "Previous", function () {
                let PreviousId = parseInt($("#PreviousId").val());

                if (PreviousId > 0) {
                    Edit@(itemName)(PreviousId);
                } else {
                    alert("已是第一筆資料!");
                }
            });

            addMethod(this, "Next", function () {
                let NextId = parseInt($("#NextId").val());

                if (NextId > 0) {
                    Edit@(itemName)(NextId);
                } else {
                    alert("已是最後一筆資料!");
                }
            });

            addMethod(this, "Save", function () {
                let formValid = [];
                formValid.push($(objForm@(itemName)).valid());

                for (var i = 0; i < json@(itemName).Details.length; i++) {
                    formValid.push($(`form[name=editDetailForm@(itemName)][data-row=${i + 1}]`).valid());
                }

                if (formValid.indexOf(false) == -1) {
                    let InterfacingId = parseInt($("#InterfacingId").val());

                    if (InterfacingId <= 0) {
                        let targetUrl = "@Url.Action("AddInterfacing", "SystemInterfacing")";
                        let postData = {
                            "InterfacingJson": JSON.stringify(json@(itemName))
                        };
                        let result = DataAccess.Add(targetUrl, postData, false, true);

                        if (result) {
                            Operations.EditClose();
                        }
                    } else {
                        let targetUrl = "@Url.Action("UpdateInterfacing", "SystemInterfacing")";
                        let postData = {
                            "InterfacingId": InterfacingId,
                            "InterfacingJson": JSON.stringify(json@(itemName))
                        };

                        let result = DataAccess.Update(targetUrl, postData, false, true);

                        if (result) {
                            Operations.EditClose();
                        }
                    }
                }
            });

            addMethod(this, "Delete", function (InterfacingId) {
                swal.fire({
                    titleText: "確認要刪除嗎?",
                    text: "此動作無法復原，請確認是否要執行!",
                    icon: "warning",
                    allowOutsideClick: false,
                    allowEscapeKey: false,
                    showCancelButton: true,
                    confirmButtonColor: '#d33',
                    reverseButtons: true
                }).then((result) => {
                    if (result.isConfirmed) {
                        let targetUrl = "@Url.Action("DeleteInterfacing", "SystemInterfacing")";
                        let postData = {
                            "InterfacingId": InterfacingId > 0 ? InterfacingId : parseInt($("#InterfacingId").val())
                        };

                        let result = DataAccess.Delete(targetUrl, postData, false, true);

                        if (result) {
                            Operations.EditClose();
                        }
                    }
                });
            });

            addMethod(this, "EditClose", function () {
                ResetForm(objForm@(itemName));
                $("#editData@(itemName)").removeClass("active");
                Query@(itemName)();
            });

            addMethod(this, "InitDetail", function () {
                $("#editData@(itemName) .datail-box").remove();

                let html = '';

                for (var i = 0; i < json@(itemName).Details.length; i++) {
                    html += `<div class="datail-box">
                               <div class="detail-box-title">
                                 <span>${i + 1}.</span>
                                 <div class="action-box">
                                   <a class="act-btn" href="javascript:void(0);" onclick="Operations.CopyDetail(${i + 1})"><i class="far fa-copy"></i>複製</a>
                                   <a class="act-btn auth-delete" href="javascript:void(0);" onclick="Operations.DeleteDetail(${i + 1})"><i class="far fa-trash-alt"></i>刪除</a>
                                 </div>
                               </div>
                               <form class="nav-form" autocomplete="off" name="editDetailForm@(itemName)" data-row="${i + 1}" data-connection-id="${json@(itemName).Details[i].ConnectionId}">
                                 <fieldset>
                                   <div class="form-group row">
                                     <label class="col-12 col-md-3 col-lg-3 col-form-label">連線主機<span class="text-danger">*</span></label>
                                     <div class="col-12 col-md-9 col-lg-9">
                                       <input type="text" class="form-control" name="txtConnectionServer@(itemName)" data-row="${i + 1}" data-connection-id="${json@(itemName).Details[i].ConnectionId}"
                                              data-msg-maxlength="必須少於 50 個字元" maxlength="50"
                                              data-msg-required="請輸入連線主機" data-rule-required="true"
                                              value="${json@(itemName).Details[i].ConnectionServer}" />
                                     </div>
                                   </div>
                                   <div class="form-group row">
                                     <label class="col-12 col-md-3 col-lg-3 col-form-label">連線帳號<span class="text-danger">*</span></label>
                                     <div class="col-12 col-md-9 col-lg-9">
                                       <input type="text" class="form-control" name="txtConnectionAccount@(itemName)" data-row="${i + 1}" data-connection-id="${json@(itemName).Details[i].ConnectionId}"
                                              data-msg-maxlength="必須少於 50 個字元" maxlength="50"
                                              data-msg-required="請輸入連線帳號" data-rule-required="true"
                                              value="${json@(itemName).Details[i].ConnectionAccount}" />
                                     </div>
                                   </div>
                                   <div class="form-group row">
                                     <label class="col-12 col-md-3 col-lg-3 col-form-label">連線密碼<span class="text-danger">*</span></label>
                                     <div class="col-12 col-md-9 col-lg-9">
                                       <input type="text" class="form-control" name="txtConnectionPassword@(itemName)" data-row="${i + 1}" data-connection-id="${json@(itemName).Details[i].ConnectionId}"
                                              data-msg-maxlength="必須少於 50 個字元" maxlength="50"
                                              data-msg-required="請輸入連線密碼" data-rule-required="true"
                                              value="${json@(itemName).Details[i].ConnectionPassword}" />
                                     </div>
                                   </div>
                                   <div class="form-group row">
                                     <label class="col-12 col-md-3 col-lg-3 col-form-label">連線資料庫</label>
                                     <div class="col-12 col-md-9 col-lg-9">
                                       <input type="text" class="form-control" name="txtConnectionDatabase@(itemName)" data-row="${i + 1}" data-connection-id="${json@(itemName).Details[i].ConnectionId}"
                                              data-msg-maxlength="必須少於 50 個字元" maxlength="50"
                                              value="${json@(itemName).Details[i].ConnectionDatabase}" />
                                     </div>
                                   </div>
                                 </fieldset>
                               </form>
                             </div>`;
                }

                $("#editData@(itemName) .datail-title").after(html);
                PageAuthorityControl();

                $(`form[name="editDetailForm@(itemName)"] .form-control`).off("change").on("change", function () {
                    let row = $(this).data("row");

                    json@(itemName).Details[row - 1].ConnectionServer = $(`input[name="txtConnectionServer@(itemName)"][data-row="${row}"]`).val();
                    json@(itemName).Details[row - 1].ConnectionAccount = $(`input[name="txtConnectionAccount@(itemName)"][data-row="${row}"]`).val();
                    json@(itemName).Details[row - 1].ConnectionPassword = $(`input[name="txtConnectionPassword@(itemName)"][data-row="${row}"]`).val();
                    json@(itemName).Details[row - 1].ConnectionDatabase = $(`input[name="txtConnectionDatabase@(itemName)"][data-row="${row}"]`).val();
                });
            });

            addMethod(this, "AddDetail", function () {
                let detail = {
                    ConnectionId: -1,
                    ConnectionServer: "",
                    ConnectionAccount: "",
                    ConnectionPassword: "",
                    ConnectionDatabase: ""
                };

                json@(itemName).Details.push(detail);

                Operations.InitDetail();
            });

            addMethod(this, "CopyDetail", function (index) {
                let detail = {
                    ConnectionId: -1,
                    ConnectionServer: json@(itemName).Details[index - 1].ConnectionServer,
                    ConnectionAccount: json@(itemName).Details[index - 1].ConnectionAccount,
                    ConnectionPassword: json@(itemName).Details[index - 1].ConnectionPassword,
                    ConnectionDatabase: json@(itemName).Details[index - 1].ConnectionDatabase
                };

                json@(itemName).Details.push(detail);

                Operations.InitDetail();
            });

            addMethod(this, "DeleteDetail", function (index) {
                swal.fire({
                    titleText: "確認要刪除嗎?",
                    text: "此動作無法復原，請確認是否要執行!",
                    icon: "warning",
                    allowOutsideClick: false,
                    allowEscapeKey: false,
                    showCancelButton: true,
                    confirmButtonColor: '#d33',
                    reverseButtons: true
                }).then((result) => {
                    if (result.isConfirmed) {
                        if (json@(itemName).Details[index - 1].ConnectionId > 0) {
                            let targetUrl = "@Url.Action("DeleteInterfacingConnection", "SystemInterfacing")";
                            let postData = {
                                "ConnectionId": json@(itemName).Details[index - 1].ConnectionId
                            };

                            let result = DataAccess.Delete(targetUrl, postData, false, true);

                            if (result) {
                                json@(itemName).Details.splice(index - 1, 1);
                                Operations.InitDetail();
                            }
                        } else {
                            json@(itemName).Details.splice(index - 1, 1);
                            Operations.InitDetail();
                        }
                    }
                });
            });
        }
    </script>
)