@using Business_Manager.Helpers
@{
    string itemName = "ViewAdvancedFunction";
    string itemNameText = "進階功能";
}

@Html.Resource("css",
    @<style>
         .table-sticky thead .col-sticky:nth-last-child(1) {
             right: 0;
         }

         .table-sticky tbody .col-sticky:nth-last-child(1) {
             right: 0;
         }

         .control {
             margin-bottom: 0px;
         }
    </style>
                )
@*<span data-key="workshop">車間</span>*@

<div class="modal fade advanced-section" style="" id="modal@(itemName)">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button class="go-back " data-dismiss="modal" type="button">
                    <span aria-hidden="true"><i class="fas fa-arrow-left"></i></span>
                </button>
                <div class="input-group" style="">
                    <div class="col-lg-2 col-md-2 col-6 pl-1 pr-1">
                        <h6 data-key="PleaseEnterSearchInformation">請輸入搜尋資訊</h6>
                    </div>
                    <div class="col-lg-2 col-md-2 col-6 pl-1 pr-1">
                        <input type="text" class="form-control form-dark" id="txtBarcodeNoQ@(itemName)" name="" data-key="PleaseEnterBarcode"  placeholder="請輸入條碼">
                    </div>
                    <div class="col-lg-2 col-md-2 col-6 pl-1 pr-1" style="margin-top: 2px;">
                        <a class="btn btn-primary ml-1" data-backdrop="static" data-keyboard="false" href="javascript:Query@(itemName)();">
                            <i class="fal fa-search"></i>
                        </a>
                    </div>
                </div>
            </div>
            <div class="modal-body" id="listData@(itemName)" style="padding:0;max-height:100vh;">
                <div class="table-outer table-advanced">
                    <table class="table-platform-sticky" id="tblNotLotData@(itemName)">
                        <thead>
                            <tr class="headerrow">
                                <th scope="col" class="text-center" style="min-width: 115px;">
                                    <label class="col-12 col-form-label">
                                        <span data-key="all">全選</span>
                                         <input type="checkbox" id="cbxNotLotSelectAll@(itemName)" />
                                    </label>
                                </th>
                                <th data-key="barcode" scope="col" width="20%">條碼</th>
                                <th data-key="barcodestatus" scope="col" width="15%">狀態</th>
                                <th data-key="quantity" scope="col" width="10%">數量</th>
                                <th data-key="NextStationProcess" scope="col" width="15%">下製程</th>
                                <th data-key="starttime" scope="col" width="20%">開始時間</th>
                                <th data-key="endtime" scope="col" width="20%">結束時間</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>

                    <table class="table-platform-sticky" id="tblLotData@(itemName)" style="display: none;">
                        <thead>
                            <tr class="headerrow">
                                <th data-key="barcode" scope="col" width="20%">條碼</th>
                                <th data-key="barcodestatus" scope="col" width="15%">狀態</th>
                                <th data-key="quantity" scope="col" width="10%">數量</th>
                                <th data-key="NextStationProcess" scope="col" width="15%">下製程</th>
                                <th data-key="starttime" scope="col" width="20%">開始時間</th>
                                <th data-key="endtime" scope="col" width="20%">結束時間</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>

                    <table class="table-platform-sticky" id="tblNotBarcodeCtrlData@(itemName)" style="display: none;">
                        <thead>
                            <tr class="headerrow">
                                <th data-key="station" scope="col">站別</th>
                                <th data-key="barcodestatus" scope="col">狀態</th>
                                <th data-key="quantity" scope="col">數量</th>
                                <th data-key="NextStationProcess" scope="col">下製程</th>
                                <th data-key="Reason" scope="col">原因</th>
                                <th data-key="SwipePersonnel" scope="col">刷取人員</th>
                                <th data-key="SwipeDate" scope="col">刷取日期</th>
                                <th data-key="operation" scope="col" class="text-center col-sticky">操作</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
                <div class="close-block" style="">
                    <button class="close" data-dismiss="modal" type="button">
                        <span aria-hidden="true"><i class="fas fa-times"></i></span>
                    </button>
                </div>
            </div>
        </div>
    </div>
</div>

