@Html.Resource("js",
@<script>
    let objForm@(itemName) = "#editForm@(itemName)";
    let objForm2@(itemName) = "#editForm2@(itemName)";
    let objForm3@(itemName) = "#editForm3@(itemName)";

    let moId@(itemName);
    let barcodeCtrl@(itemName);
    let moProcessId@(itemName);
    let lotStatus@(itemName);
    let machineId@(itemName);
    let barcodeProcessIdList@(itemName) = [];
    let barcodeNoList@(itemName) = [];

    function Pop@(itemName)(moId, barcodeCtrl, moProcessId, lotStatus, machineId) {
        moId@(itemName) = moId;
        barcodeCtrl@(itemName) = barcodeCtrl;
        moProcessId@(itemName) = moProcessId;
        lotStatus@(itemName) = lotStatus;
        machineId@(itemName) = machineId;

        if (!isMobile) {
            $("#cboProdModeQ@(itemName)").select2({
                width: "100%"
            });
        }

        Query@(itemName)();

        $("#modal@(itemName)").modal({
            backdrop: "static",
            keyboard: false,
            show: true
        });

        $("#LotBarcodeNgCode@(itemName)").modal({
            backdrop: "static",
            keyboard: false,
            show: false
        });

        $("#modalQuantityChange@(itemName)").modal({
            backdrop: "static",
            keyboard: false,
            show: false
        });

        $("#cbxNotLotSelectAll@(itemName)").prop("checked", false);

        $("#cbxNotLotSelectAll@(itemName)").off("click").on("click", function () {
            if ($("#cbxNotLotSelectAll@(itemName)").is(":checked")) {
                $("input[type='checkbox'][data-barcode-no]").not(":disabled").prop("checked", true);
            } else {
                $("input[type='checkbox'][data-barcode-no]").not(":disabled").prop("checked", false);
            }
        });
    }

    function Query@(itemName)() {
        $("#cbxNotLotSelectAll@(itemName)").prop("checked", false);
        $("#cbxLotSelectAll@(itemName)").prop("checked", false);
        InitData@(itemName)();
    }

    let barcodeInfoJson =
    {
        barcodeInfo: []
    };
    function InitData@(itemName)() {
        let innerHtml = '';
        let targetUrl = "";
        let postData;
        let data;
        if (barcodeCtrl@(itemName) == "Y" && lotStatus@(itemName) == "N") { //有條碼且為非批量條碼 EX:模仁
            $("#tblNotLotData@(itemName)").show();
            $("#tblLotData@(itemName)").hide();
            $("#tblNotBarcodeCtrlData@(itemName)").hide();

            targetUrl = "@Url.Action("GetBarcodeProcess", "ProductionHistory")";
            postData = {
                "MoProcessId": moProcessId@(itemName),
                "BarcodeNo": $("#txtBarcodeNoQ@(itemName)").val()
            };
            data = DataAccess.Get(targetUrl, postData, false);

            if (data.result.length > 0) {
                let trClass = "";
                $.each(data.result, function (i, item) {
                    if (i % 2 == 0) trClass = "line-odd";
                    else trClass = "line-even";
                    //<option value="S" ${item.CurrentProdStatus == `S` ? `selected` : ``}>報廢</option>
                    innerHtml += `<tr class="${trClass}">
                                     <td class="text-center">
                                        <label class="control control-solid control-solid-info control--checkbox">
                                            ${i + 1}. <input type="checkbox" data-barcode-no="${item.BarcodeNo}" value="${item.BarcodeProcessId}" />
                                            <span class="control__indicator"></span>
                                        </label>
                                    </td>
                                    <th>
                                        <p>${item.BarcodeNo}</p>
                                        ${JudgeTrayBarcode@(itemName)(`${item.TrayBarcode}`, `${item.TrayNo}`)}
                                        ${JudgeItemValue@(itemName)(`${item.ItemValue}`)}
                                    </th>
                                    <td>
                                        <select id="cboProdStatus@(itemName)" class="form-control" onChange="NotLotChangeProdStatus@(itemName)('${item.BarcodeProcessId}', this, '${item.BarcodeNo}')">
                                            <option value="P" ${item.CurrentProdStatus == `P` ? `selected` : ``}>良品</option>
                                            <option value="F" ${item.CurrentProdStatus == `F` ? `selected` : ``}>不良品</option>
                                            <option value="S" ${item.CurrentProdStatus == `S` ? `selected` : ``}>報廢品</option>
                                        </select>
                                    </td>
                                    <td>${item.StationQty}</td>
                                    <td>
                                        <select id="cboNextMoProcess@(itemName)" class="form-control" data-barcode-no="${item.BarcodeNo}" data-next-mo-process-id="${item.NextMoProcessId}" onChange="ChangeNextMoProcessFnc@(itemName)('${item.BarcodeNo}', this)"></select>
                                    </td>
                                    <td>${item.StartDate}</td>
                                    <td>${item.FinishDate}</td>
                                  </tr>`;

                    function JudgeTrayBarcode@(itemName)(trayBarcode, trayNo) {
                        let innerHtml = '';
                        if (trayBarcode == "Y") {
                            innerHtml += `<code>(${trayNo})</code>`;
                        }
                        return innerHtml;
                    }

                    function JudgeItemValue@(itemName)(itemValue) {
                        let innerHtml = '';
                        if (itemValue != "null" && itemValue != null) {
                            let itemValueJson = JSON.parse(itemValue);
                            let dateCode = "";
                            $.each(itemValueJson["data"], function (k, item2) {
                                innerHtml += `<code>${item2.ItemValue}</code><br>`;

                                if (k == 0) {
                                    dateCode = item2.DateCode;
                                }

                                if (k == itemValueJson["data"].length - 1) {
                                    innerHtml += `<code>${dateCode}</code><br>`;
                                }
                            });
                        }

                        return innerHtml;
                    }
                });
            }

            $("#tblNotLotData@(itemName) tbody").html(innerHtml);
            TableComponentInit();

            $.each($("select[id='cboNextMoProcess@(itemName)']"), function (i) {
                let barcodeNo = $(this).data("barcode-no");
                let nextMoProcessId = $(this).data("next-mo-process-id");
                ComboBoxs.Default("select[id='cboNextMoProcess@(itemName)'][data-barcode-no='" + barcodeNo + "']", "@Url.Action("GetMoProcess", "ProductionHistory")", { "MoId": moId@(itemName) }, "" + nextMoProcessId + "", "NA", "", "ProcessAlias", "MoProcessId");
                $("select[id='cboNextMoProcess@(itemName)'][data-barcode-no='" + barcodeNo + "']").append(new Option("完工", "-1"));
                $("select[id='cboNextMoProcess@(itemName)'][data-barcode-no='" + barcodeNo + "']").val(nextMoProcessId);

            });
        }
        else if (barcodeCtrl@(itemName) == "Y" && lotStatus@(itemName) == "Y") { //有條碼且為批量條碼 EX:成型批量條碼
            $("#tblNotLotData@(itemName)").hide();
            $("#tblLotData@(itemName)").show();
            $("#tblNotBarcodeCtrlData@(itemName)").hide();

            targetUrl = "@Url.Action("GetBarcodeProcess", "ProductionHistory")";
            postData = {
                "MoProcessId": moProcessId@(itemName),
                "BarcodeNo": $("#txtBarcodeNoQ@(itemName)").val()
            };
            data = DataAccess.Get(targetUrl, postData, false);

            if (data.result.length > 0) {
                let trClass = "";
                $.each(data.result, function (i, item) {
                    if (i % 2 == 0) trClass = "line-odd";
                    else trClass = "line-even";

                    innerHtml += `<tr class="${trClass}">
                                    <th>
                                        <p>${item.BarcodeNo}</p>
                                        ${JudgeTrayBarcode@(itemName)(`${item.TrayBarcode}`, `${item.TrayNo}`)}
                                        ${JudgeItemValue@(itemName)(`${item.ItemValue}`)}
                                    </th>
                                    <td>
                                        <select id="cboProdStatus@(itemName)" class="form-control" onChange="LotChangeProdStatus@(itemName)('${item.NgToBarcode}', '${item.BarcodeNo}', '${item.StationQty}', '${item.ProdStatus}', this, '${item.BarcodeProcessId}', '${item.MinBarcodes}')">
                                            <option value="P" ${item.CurrentProdStatus == `P` ? `selected` : ``}>良品</option>
                                            <option value="F" ${item.CurrentProdStatus == `F` ? `selected` : ``}>不良品</option>
                                            <option value="S" ${item.CurrentProdStatus == `S` ? `selected` : ``}>報廢品</option>
                                        </select>
                                    </td>
                                    <td>${item.StationQty}</td>
                                    <td>
                                        <select id="cboNextMoProcess@(itemName)" class="form-control" data-barcode-no="${item.BarcodeNo}" data-next-mo-process-id="${item.NextMoProcessId}" onChange="ChangeNextMoProcessFnc@(itemName)('${item.BarcodeNo}', this)"></select>
                                    </td>
                                    <td>${item.StartDate}</td>
                                    <td>${item.FinishDate}</td>
                                  </tr>`;

                    function JudgeTrayBarcode@(itemName)(trayBarcode, trayNo) {
                        let innerHtml = '';
                        if (trayBarcode == "Y") {
                            innerHtml += `<code>(${trayNo})</code>`;
                        }
                        return innerHtml;
                    }

                    function JudgeItemValue@(itemName)(itemValue) {
                        let innerHtml = '';
                        if (itemValue != "null") {
                            let itemValueJson = JSON.parse(itemValue);
                            $.each(itemValueJson["data"], function (k, item2) {
                                innerHtml += `<code>${item2.ItemValue}</code><br>`;
                            });
                        }

                        return innerHtml;
                    }
                });
            }

            $("#tblLotData@(itemName) tbody").html(innerHtml);
            TableComponentInit();

            $.each($("select[id='cboNextMoProcess@(itemName)']"), function (i) {
                let barcodeNo = $(this).data("barcode-no");
                let nextMoProcessId = $(this).data("next-mo-process-id");
                ComboBoxs.Default("select[id='cboNextMoProcess@(itemName)'][data-barcode-no='" + barcodeNo + "']", "@Url.Action("GetMoProcess", "ProductionHistory")", { "MoId": moId@(itemName) }, "" + nextMoProcessId + "", "NA", "", "ProcessAlias", "MoProcessId");
                $("select[id='cboNextMoProcess@(itemName)'][data-barcode-no='" + barcodeNo + "']").append(new Option("完工", "-1"));
                $("select[id='cboNextMoProcess@(itemName)'][data-barcode-no='" + barcodeNo + "']").val(nextMoProcessId);
            });
        }
        else { //無條碼 EX:模造數量過站
            $("#tblNotLotData@(itemName)").hide();
            $("#tblLotData@(itemName)").hide();
            $("#tblNotBarcodeCtrlData@(itemName)").show();

            targetUrl = "@Url.Action("GetMoProcessTransaction", "ProductionHistory")";
            postData = {
                "MoProcessId": moProcessId@(itemName)
            };
            data = DataAccess.Get(targetUrl, postData, false);

            if (data.result.length > 0) {
                let trClass = "";
                $.each(data.result, function (i, item) {
                    if (i % 2 == 0) trClass = "line-odd";
                    else trClass = "line-even";
                    innerHtml += `<tr class="${trClass}">
                                    <th><p>${item.ProcessAlias != null ? item.ProcessAlias : ``}</p></th>
                                    <td>${item.ProdStatus}</td>
                                    <td>${item.Quantity}</td>
                                    <td>${item.NextProcessAlias != null ? item.NextProcessAlias : ``}</td>
                                    ${JudgeCauseNo@(itemName)(`${item.CauseNo}`, `${item.CauseName}`)}
                                    <td>${item.UserNo} ${item.UserName}</td>
                                    <td>${item.InputDate}</td>
                                    <td class="text-center col-sticky">
                                        <a class="active-link" href="javascript: JudgeQuantity@(itemName)('${item.TransactionId}', '${item.ProcessAlias}', '${item.ProdStatus}', '${item.Quantity}', '${item.NextProcessAlias}', '${item.CauseNo}');"><i class="fas fa-user-check"></i> 改判</a>
                                    </td>
                                  </tr>`;
                });

                function JudgeCauseNo@(itemName)(causeNo, causeName) {
                    let innerHtml = '';
                    if (causeNo != "null") {
                        innerHtml += `<td>${causeNo}-${causeName}</td>`;
                    }
                    else {
                        innerHtml += `<td></td>`;
                    }
                    return innerHtml;
                }
            }

            $("#tblNotBarcodeCtrlData@(itemName) tbody").html(innerHtml);
            TableComponentInit();

            ComboBoxs.Default("select[id='cboQuantityNextMoProcess@(itemName)']", "@Url.Action("GetMoProcess", "ProductionHistory")", { "MoId": moId@(itemName) }, "", "NA", "", "ProcessAlias", "MoProcessId");
        }
    }

    $("#txtCauseNo@(itemName)").keydown(function (event) {
        if (event.keyCode == 13) {
            SaveLotNgBarcode@(itemName)();
        }
    });

    function ChangeNextMoProcess@(itemName)(barcodeId, barcodeNo, moProcessId, processAlias) {
        $("#desBarcodeNo@(itemName)").html(barcodeNo);
        $("#desProcessAlias@(itemName)").html(processAlias);
        $("#modalChangeNextMoProcess@(itemName)").show();
    }

    function ChangeNextMoProcessFnc@(itemName)(barcodeNo, e) {
        swal.fire({
            titleText: "確認要更改下製程嗎?",
            text: "此動作無法復原，請確認是否要執行!",
            icon: "warning",
            allowOutsideClick: false,
            allowEscapeKey: false,
            showCancelButton: true,
            confirmButtonColor: '#d33',
            reverseButtons: true
        }).then((result) => {
            if (result.isConfirmed) {

                barcodeNoList@(itemName) = [];
                $.each($("input[type='checkbox'][data-barcode-no]:checked"), function (i) {
                    let barcodeNo = $(this).data("barcode-no");
                    $.each(barcodeNoList@(itemName), function (i, item) {
                        if (item == barcodeNo) {
                            barcodeNoList@(itemName).splice(i, 1);
                            return false;
                        }
                    });

                    barcodeNoList@(itemName).push(barcodeNo);
                });

                if (barcodeNoList@(itemName).length <= 0) barcodeNoList@(itemName).push(barcodeNo);


                let targetUrl = "@Url.Action("UpdateNextMoProcessForLotMode", "ProductionHistory")";
                let postData = {
                    "BarcodeNoListString": barcodeNoList@(itemName).toString(),
                    "MoProcessId": moProcessId@(itemName),
                    "NewMoProcessId": $(e).val()
                };

                let result = DataAccess.Update(targetUrl, postData, false, true);

                Query@(itemName)();
                ReturnMesScanningInterface();
            }
            else {
                Query@(itemName)();
            }
        });
    }

    function NotLotChangeProdStatus@(itemName)(barcodeProcessId, e, barcodeNo) {
        let prodStatus = $(e).val();

        barcodeProcessIdList@(itemName) = [];
        barcodeNoList@(itemName) = [];
        $.each($("input[type='checkbox'][data-barcode-no]:checked"), function (i) {
            let barcodeProcessId = $(this).val();
            let barcodeNo = $(this).data("barcode-no");
            $.each(barcodeProcessIdList@(itemName), function (i, item) {
                if (item == barcodeProcessId) {
                    barcodeProcessIdList@(itemName).splice(i, 1);
                    return false;
                }
            });

            $.each(barcodeNoList@(itemName), function (i, item) {
                if (item == barcodeNo) {
                    barcodeNoList@(itemName).splice(i, 1);
                    return false;
                }
            });

            barcodeProcessIdList@(itemName).push(barcodeProcessId);
            barcodeNoList@(itemName).push(barcodeNo);
        });

        if (barcodeProcessIdList@(itemName).length <= 0) barcodeProcessIdList@(itemName).push(barcodeProcessId);
        if (barcodeNoList@(itemName).length <= 0) barcodeNoList@(itemName).push(barcodeNo);

        if (prodStatus != "F") {
            swal.fire({
                titleText: "確認要更改條碼狀態嗎?",
                text: "此動作無法復原，請確認是否要執行!",
                icon: "warning",
                allowOutsideClick: false,
                allowEscapeKey: false,
                showCancelButton: true,
                confirmButtonColor: '#d33',
                reverseButtons: true
            }).then((result) => {
                if (result.isConfirmed) {
                    let targetUrl = "@Url.Action("UpdateProdStatusForLotMode", "ProductionHistory")";
                    let postData = {
                        "BarcodeProcessIdListString": barcodeProcessIdList@(itemName).toString(),
                        "ProdStatus": prodStatus
                    };

                    let result = DataAccess.Update(targetUrl, postData, false, true);

                    Query@(itemName)();
                    ReturnMesScanningInterface();
                }
                else {
                    Query@(itemName)();
                }
            });
        }
        else {
            $("#txtNotLotNgCodeBarcodeProcessId@(itemName)").val(barcodeProcessId);
            $("#desNotLotNgCodeBarcodeNo@(itemName)").html(barcodeNoList@(itemName).toString());
            $("#desNotLotNgCodeProdStatus@(itemName)").html(prodStatus);
            $("#modalNotLotNgCode@(itemName)").modal("show");
        }
    }

    function LotChangeProdStatus@(itemName)(ngToBarcode, barcodeNo, barcodeQty, prodStatus, e, barcodeProcessId, minBarcodes) {
        if (minBarcodes == "1") {
            $(".min-barcodes").show();
            $("#txtNgBarcodeQty@(itemName)").prop("readonly", true);
            $("#cboMinBarcodes@(itemName)").prop("required", true);
        }
        else {
            $(".min-barcodes").hide();
            $("#txtNgBarcodeQty@(itemName)").prop("readonly", false);
            $("#cboMinBarcodes@(itemName)").prop("required", false);
        }

        $("#cboMinBarcodes@(itemName)").select2({
            dropdownAutoWidth: true,
            width: "100%"
        });

        ComboBoxs.Default("#cboMinBarcodes@(itemName)", "@Url.Action("GetMinBarcodes", "ProductionHistory")", { "BarcodeNo": barcodeNo }, "", "CHOOSE-NUMBER", "", "BarcodeNo", "BarcodeId");
        $("#aClearMinBarcodes@(itemName)").prop("href", `javascript:ClearMinBarcodes@(itemName)('${barcodeNo}');`)

        $("#cboMinBarcodes@(itemName)").change(function () {
            let totalQty = $("#cboMinBarcodes@(itemName)").val().length;
            $("#txtNgBarcodeQty@(itemName)").val(totalQty);
        });

        if (ngToBarcode == "T") {
            NotLotChangeProdStatus@(itemName)(barcodeProcessId, e, barcodeNo);
            return false;
        }

        let userProdStatus = $(e).val();
        $("#txtBarcodeProcessId@(itemName)").val(barcodeProcessId);
        $("#desBarcodeNo@(itemName)").html(barcodeNo);
        $("#desBarcodeQty@(itemName)").html(barcodeQty);
        $("#desProdStatus@(itemName)").html(prodStatus);
        $("#desUserProdStatus@(itemName)").html(userProdStatus);

        if (ngToBarcode == "Y") {
            $("#txtNgBarcodeNo@(itemName)").prop("readonly", false);
            $("#txtNgBarcodeNo@(itemName)").prop("required", true);
        }
        else {
            $("#txtNgBarcodeNo@(itemName)").prop("readonly", true);
            $("#txtNgBarcodeNo@(itemName)").prop("required", false);
        }

        if (userProdStatus == "P") {
            //alert("不良品改判回良品功能暫不開放。");
            //return false;
            $("#txtCauseNo@(itemName)").prop("readonly", true);
            $("#txtCauseNo@(itemName)").prop("required", false);

            $("#txtNgBarcodeNo@(itemName)").prop("readonly", true);
            $("#txtNgBarcodeNo@(itemName)").prop("required", false);

            $("#txtPassBarcodeNo@(itemName)").prop("readonly", false);
            $("#txtPassBarcodeNo@(itemName)").prop("required", true);

            $("#txtCauseNo@(itemName)").val("改判為良品");

            $("#LotBarcodeNgCode@(itemName)").modal("show");
        }
        else if (userProdStatus == "F") {
            $("#txtCauseNo@(itemName)").prop("readonly", false);
            $("#txtCauseNo@(itemName)").prop("required", true);

            $("#txtPassBarcodeNo@(itemName)").prop("readonly", true);
            $("#txtPassBarcodeNo@(itemName)").prop("required", false);

            $("#LotBarcodeNgCode@(itemName)").modal("show");
        }
        else {
            alert("修改報廢狀態目前未開發!");
            Query@(itemName)();
        }
    }

    function ClearMinBarcodes@(itemName)(barcodeNo) {
        $("#cboMinBarcodes@(itemName)").empty();
        ComboBoxs.Default("#cboMinBarcodes@(itemName)", "@Url.Action("GetMinBarcodes", "ProductionHistory")", { "BarcodeNo": barcodeNo }, "", "CHOOSE-NUMBER", "", "BarcodeNo", "BarcodeId");
        $("#txtNgBarcodeQty@(itemName)").val("0");
    }

    function SaveLotNgBarcode@(itemName)() {
        if ($(objForm@(itemName)).valid()) {
            let userProdStatus = $("#desUserProdStatus@(itemName)").text();
            let barcodeProcessId = $("#txtBarcodeProcessId@(itemName)").val();
            if (userProdStatus == "P") {
                let targetUrl = "@Url.Action("UpdateLotNgBarcodeProcessToPass", "ProductionHistory")";
                let postData = {
                    "BarcodeProcessId": barcodeProcessId,
                    "PreBarcodeNo": $("#txtPassBarcodeNo@(itemName)").val(),
                    "ChangeBarcodeQty": $("#txtNgBarcodeQty@(itemName)").val(),
                    "MoProcessId": moProcessId@(itemName),
                    "MinBarcodes": $("#cboMinBarcodes@(itemName)").val().join(",")
                };

                let result = DataAccess.Add(targetUrl, postData, false, true);

                if (result) {
                    Close@(itemName)('LotBarcodeNgCode@(itemName)');
                    ReturnMesScanningInterface();
                }
            }
            else {
                let targetUrl = "@Url.Action("AddLotNgBarcodeProcess", "ProductionHistory")";
                let postData = {
                    "BarcodeNo": $("#desBarcodeNo@(itemName)").text(),
                    "NgBarcodeNo": $("#txtNgBarcodeNo@(itemName)").val(),
                    "NgBarcodeQty": $("#txtNgBarcodeQty@(itemName)").val(),
                    "MoId": moId@(itemName),
                    "MachineId": machineId@(itemName),
                    "CauseNo": $("#txtCauseNo@(itemName)").val(),
                    "MoProcessId": moProcessId@(itemName),
                    "MinBarcodes": $("#cboMinBarcodes@(itemName)").val().join(",")
                };

                let result = DataAccess.Add(targetUrl, postData, false, true);

                if (result) {
                    Close@(itemName)('LotBarcodeNgCode@(itemName)');
                    ReturnMesScanningInterface();
                }
            }
        }
    }

    function SaveNotLotNgCode@(itemName)() {
        if ($(objForm3@(itemName)).valid()) {

            barcodeProcessIdList@(itemName) = [];
            $.each($("input[type='checkbox'][data-barcode-no]:checked"), function (i) {
                let barcodeProcessId = $(this).val();
                $.each(barcodeProcessIdList@(itemName), function (i, item) {
                    if (item == barcodeProcessId) {
                        barcodeProcessIdList@(itemName).splice(i, 1);
                        return false;
                    }
                });

                barcodeProcessIdList@(itemName).push(barcodeProcessId);
            });

            if (barcodeProcessIdList@(itemName).length <= 0) barcodeProcessIdList@(itemName).push($("#txtNotLotNgCodeBarcodeProcessId@(itemName)").val());

            let targetUrl = "@Url.Action("UpdateProdStatusForLotMode", "ProductionHistory")";
            let postData = {
                "BarcodeProcessIdListString": barcodeProcessIdList@(itemName).toString(),
                "ProdStatus": $("#desNotLotNgCodeProdStatus@(itemName)").text(),
                "CauseNo": $("#txtNotLotNgCodeCauseNo@(itemName)").val()
            };

            let result = DataAccess.Update(targetUrl, postData, false, true);

            if (result) {
                Close@(itemName)('modalNotLotNgCode@(itemName)');
                Query@(itemName)();
                ReturnMesScanningInterface();
            }
        }
    }

    function EditDefectCauseNo@(itemName)(id) {
        clearTimeout(timer)
        timer = setTimeout(function () {
            let defectCauseNo = $("#" + id + "").val();
            defectCauseNo = defectCauseNo.toUpperCase();
            let targetUrl = "@Url.Action("GetNgCause", "QmsBasicInformation")";
            let postData = {
                "CauseNo": defectCauseNo
            };
            let data = DataAccess.Get(targetUrl, postData, false);

            if (data.status == "success") {
                if (data.result.length <= 0) {
                    alert("找不到此NG代碼!");
                    $("#" + id + "").val("");
                }
                else {
                    $("#" + id + "").val(defectCauseNo);
                }
            }
            else {
                alert(data.msg);
            }
        }, 3500);
    }

    function JudgeQuantity@(itemName)(transactionId, processAlias, prodStatus, quantity, nextProcessAlias, causeNo) {
        if (processAlias == "null") processAlias = "";
        if (nextProcessAlias == "null") nextProcessAlias = "";
        if (causeNo == "null") causeNo = "";

        $("#txtTransactionId@(itemName)").val(transactionId);

        $("#desQuantityProcessAlias@(itemName)").html(processAlias);
        $("#desQuantityProdStatus@(itemName)").html(prodStatus);
        $("#desQuantityChangeQty@(itemName)").html(quantity);
        $("#desQuantityNextProcessAlias@(itemName)").html(nextProcessAlias);
        $("#desQuantityCauseNo@(itemName)").html(causeNo);
        $("#modalQuantityChange@(itemName)").modal("show");

        $("#cboJudgeType@(itemName)").change(function () {
            let judgeType = $("#cboJudgeType@(itemName)").val();
            if (judgeType == "P") {
                $("#txtQuantityCauseNo@(itemName)").prop("readonly", true);
                $("#txtQuantityCauseNo@(itemName)").prop("required", false);

                $("#cboQuantityNextMoProcess@(itemName)").prop("disabled", true);
                $("#cboQuantityNextMoProcess@(itemName)").prop("required", false);

                $("#cboQuantityNextMoProcess@(itemName)").val("");
                $("#txtQuantityCauseNo@(itemName)").val("改判回良品");
            }
            else if (judgeType == "F") {
                $("#txtQuantityCauseNo@(itemName)").prop("readonly", false);
                $("#txtQuantityCauseNo@(itemName)").prop("required", true);

                $("#cboQuantityNextMoProcess@(itemName)").prop("disabled", false);
                $("#cboQuantityNextMoProcess@(itemName)").prop("required", true);
            }
            else if (judgeType == "S") {
                $("#txtQuantityCauseNo@(itemName)").prop("readonly", false);
                $("#txtQuantityCauseNo@(itemName)").prop("required", true);

                $("#cboQuantityNextMoProcess@(itemName)").prop("disabled", true);
                $("#cboQuantityNextMoProcess@(itemName)").prop("required", false);

                $("#cboQuantityNextMoProcess@(itemName)").val("");
            }
        });
    }

    function SaveQuantityJudge@(itemName)() {
        if ($(objForm2@(itemName)).valid()) {
            let targetUrl = "@Url.Action("UpdateQunatityJudge", "ProductionHistory")";
            let postData = {
                "TransactionId": $("#txtTransactionId@(itemName)").val(),
                "ToJudgeType": $("#cboJudgeType@(itemName)").val(),
                "ChangeQty": $("#txtQuantityChangeQty@(itemName)").val(),
                "NextMoProcessId": $("#cboQuantityNextMoProcess@(itemName)").val(),
                "CauseNo": $("#txtQuantityCauseNo@(itemName)").val()
            };

            let result = DataAccess.Update(targetUrl, postData, false, true);

            if (result) {
                Close@(itemName)('modalQuantityChange@(itemName)');
                ReturnMesScanningInterface();
            }
        }
    }

    function Return@(itemName)() {

    }

    function Close@(itemName)(modalName) {
        $("#" + modalName + "").modal('hide');
        Query@(itemName)();
        ResetForm(objForm@(itemName));
        ResetForm(objForm2@(itemName));
        ResetForm(objForm3@(itemName));
    }
</script>
