                                    <div class="col-12 col-md-9 col-lg-4">
                                        <select class="form-control" id="cboMcId@(itemName)" name="cboMcId@(itemName)" data-rule-required="true">
                                            <option value="">-- 分類 --</option>
                                        </select>
                                    </div>
