                        } else {
                            d5d6innerHtml = ``;
                        }

                        if (Stage == "D8") {
                            d8DataCount@(itemName) > 0 ? $(`#dataInsertD8@(itemName)`).attr('disabled', true) : $(`#dataInsertD8@(itemName)`).attr('disabled', false);

                            if (item.McName == "報告回覆日期後，連續三批出貨無異常，予以結案") {
                                editinnerHtml = `${item.ConfirmStatus == `N` ?
