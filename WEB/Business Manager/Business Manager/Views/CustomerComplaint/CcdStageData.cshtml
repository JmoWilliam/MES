                                              : `<a class="active-link auth-file-read active-disable" href="javascript:javascript:void(0);"><i class="fas fa-coins"></i>檔案上傳</a>`
                                             }
                                            ${item.ConfirmStatus == `N` ?
                                              `<a class="active-link auth-delete" href="javascript:Delete@(itemName)('${Stage}',${item.CcStageDataId});"><i class="fas fa-trash-alt"></i></a>`
                                             : `<a class="active-link auth-delete active-disable" href="javascript:javascript:void(0);"><i class="fas fa-trash-alt"></i></a>`
                                             }
                                            ${editinnerHtml}
                                        </td>
                                      </tr>`;
                    });
                } else {
                    $(`#dataInsertD8@(itemName)`).attr('disabled', false);
                    $(`#dataInsert${Stage}@(itemName)`).attr('disabled', false);
                    $(`#dataCheck${Stage}@(itemName)`).attr('disabled', true);
                    $(`#dataUncheck${Stage}@(itemName)`).attr('disabled', true);
                    $(`#dataDelete${Stage}@(itemName)`).attr('disabled', false);
