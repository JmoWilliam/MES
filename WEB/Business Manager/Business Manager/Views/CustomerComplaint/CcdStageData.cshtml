            });

        //$("#pets :selected").text();


        function Expand@(itemName)() {
            if (parseInt($("#CcId").val()) > 0) {
                $(".advanced-block").slideDown("slow");
                GetUserInfo@(itemName)()


                @*$("#cboUserId@(itemName)").change(function () {
                    var cboUserId = $("#cboUserId@(itemName)").val();
                    ComboBoxs.Default("#cboDeptId@(itemName)", "@Url.Action("GetDepartment_A", "EipBasicInformation")", { "UserId": cboUserId }, "", "DepartmentName", "", "DepartmentName", "DepartmentId");
                });*@

                Return@(itemName)();
            } else {
                alert("客訴單據資料錯誤", "alert");
            }
        }

        function Query@(itemName)() {
            objPage@(itemName).pageIndex = 0;
            InitData@(itemName)(objPage@(itemName));

            $("#modalQuery@(itemName)").modal("hide");
        }

        function InitData@(itemName)(objPage) {
            var StageDataSetting = ""
            let targetUrl = "@Url.Action("GetCustomerComplaint", "CustomerComplaint")";
            let postData = {
                "CcId": $("#CcId").val()
            };

            let data = DataAccess.Get(targetUrl, postData, false);

            if (data.status == "success") {
                $.each(data.result, function (i, item) {
                    //基本資料
                    StageDataSetting = item.StageDataSetting
                });
            } else {
                alert(data.msg, "alert", 0);
            }
            for (var i = 0; i < StageDataSetting.length; i++) {
                if (StageDataSetting[i] == "N") {
                    $(`#basicDataD${i+1}@(itemName)`).hide()
                }
                else {
                    $(`#basicDataD${i+1}@(itemName)`).show()
                    if (i == 0) {
                        GetCCMember@(itemName)()
                    }
                    else {
                        GetStage@(itemName)(`D${i+1}`)
                    }
                }
            }
        }

        function Edit@(itemName)(Stage, CcStageDataId) {
            $("#Stage").val(Stage);

            $("#CcStageDataId").val(CcStageDataId ? CcStageDataId : 0);

            $("#cboMcId@(itemName)").attr("disabled", false);
            $("#txtConfirmDate@(itemName)").attr("disabled", true);
            $("#cboDeptId@(itemName)").attr("disabled", false);
            $("#cboUserId@(itemName)").attr("disabled", false);
            $("#txtRemark@(itemName)").attr("readonly", false);

            ComboBoxs.Default("#cboMcId@(itemName)", "@Url.Action("GetMenuCategorySimple", "CustomerComplaint")", { "McType": Stage }, "", "NA", "", "McName", "McId");
            ComboBoxs.Default("#cboDeptId@(itemName)", "@Url.Action("GetDepartment", "BasicInformation")", { "Status": "A" }, "", "NA", "", "DepartmentWithNo", "DepartmentId");
            ComboBoxs.Default("#cboUserId@(itemName)", "@Url.Action("GetUser", "BasicInformation")", { "Status": "A" }, "", "NA", "", "UserWithNo", "UserId");



            if (Stage == "D5") {
                $("#forD5@(itemName)").show();
                $("#forD6@(itemName)").hide();
            } else if (Stage == "D6") {
                $("#forD5@(itemName)").hide();
                $("#forD6@(itemName)").show();
            } else {
                $("#forD5@(itemName)").hide();
                $("#forD6@(itemName)").hide();
            }
            @*Switch("#editData@(itemName), .unit-block", "#listData@(itemName)");*@
            $("#edutmodalQuery@(itemName)").modal({
                backdrop: "static",
                keyboard: false,
                show: true
            });


            if (CcStageDataId > 0) {
                let targetUrl = "@Url.Action("GetCcStageData", "CustomerComplaint")";
                let postData = {
                    "CcStageDataId": CcStageDataId,
                    "Stage": Stage
                };

                let data = DataAccess.Get(targetUrl, postData, false);

                if (data.status == "success") {
                    $.each(data.result, function (i, item) {
                        //基本資料

                        $("#cboMcId@(itemName)").val(item.McId);
                        $("#txtConfirmDate@(itemName)").val(item.ConfirmDate);
                        $("#cboDeptId@(itemName)").val(item.DeptId);
                        $("#cboUserId@(itemName)").val(item.UserId);
                        $("#txtDescriptionTextarea@(itemName)").val(item.DescriptionTextarea);
                    });
                } else {
                    alert(data.msg, "alert", 0);
                }
            } else {
                $("#txtDescriptionTextarea@(itemName)").val("");
            }
        


            if (!isMobile) {
                $("#cboMcId@(itemName), #cboDeptId@(itemName), #cboUserId@(itemName)").select2({
                    width: "100%"
                });
            }

        }

        function Save@(itemName)() {
            if ($(objForm@(itemName)).valid()) {
                let CcStageDataId = parseInt($("#CcStageDataId").val());
                let stage = $("#Stage").val();
                let expectedDate = "";
                let finishDate = "";

                if (stage == "D5") {
                    expectedDate = $("#txtExpectedDateD5@(itemName)").val();
                } else if (stage == "D6") {
                    finishDate = $("#txtFinishDateD6@(itemName)").val();

                }

                if (CcStageDataId <= 0) {
                    console.log("w5")

                    let targetUrl = "@Url.Action("AddCcStageData", "CustomerComplaint")";
                    let postData = {
                        "CcId": $("#CcId").val(),
                        "Stage": $("#Stage").val(),
                        "McId": $("#cboMcId@(itemName)").val(),
                        "DeptId": $("#cboDeptId@(itemName)").val(),
                        "UserId": $("#cboUserId@(itemName)").val(),
                        "DescriptionTextarea": $("#txtDescriptionTextarea@(itemName)").val(),
                        "ExpectedDate": expectedDate,
                        "FinishDate": finishDate
                    };

                    let result = DataAccess.Add(targetUrl, postData, false, true);

                    if (result) {
                        Return@(itemName)();
                        $("#edutmodalQuery@(itemName)").modal("hide");
                    }
                } else {
                    let targetUrl = "@Url.Action("UpdateCcStageData", "CustomerComplaint")";
                    let postData = {
                        "CcStageDataId": $("#CcStageDataId").val(),
