@using Business_Manager.Helpers
@{
    string itemName = "WoModificationManagement";
    string itemNameText = "ERP製令變更單管理";
    string authority = ViewBag.authority;
    ViewBag.Title = itemNameText;
}

@Html.Resource("css",
    @<style>
    </style>
                )

<input type="hidden" id="pageAuthority" value="@authority" />

<div class="row">
    <div class="col-12 title-block">
        <h1 class="pt-2 pb-3">@(itemNameText)</h1>
        <div class="unit-block">
            <div class="button-block mb-1">
                <button type="button" class="btn btn-secondary" onclick="Return@(itemName)()"><i class="fas fa-undo"></i>返回</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="modalQuery@(itemName)">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">搜尋</h5>
                <button class="close" data-dismiss="modal" type="button">
                    <span aria-hidden="true"><i class="fas fa-times"></i></span>
                </button>
            </div>
            <div class="modal-body">
                <form class="container" autocomplete="off" id="queryForm@(itemName)">
                    <fieldset>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="txtDeviceNameQ@(itemName)">設備名稱</label>
                            <div class="col-12">
                                <input type="text" class="form-control" id="txtDeviceNameQ@(itemName)" name="txtDeviceNameQ@(itemName)" placeholder="請輸入設備名稱" />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="txtDeviceIdentifierCodeQ@(itemName)">設備識別碼</label>
                            <div class="col-12">
                                <input type="text" class="form-control" id="txtDeviceIdentifierCodeQ@(itemName)" name="txtDeviceIdentifierCodeQ@(itemName)" placeholder="請輸入設備識別碼" />
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="cboDeviceTypeQ@(itemName)">設備類別</label>
                            <div class="col-12">
                                <select class="form-control" id="cboDeviceTypeQ@(itemName)" name="cboDeviceTypeQ@(itemName)"></select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label" for="cboDeviceAuthorityQ@(itemName)">設備權限</label>
                            <div class="col-12">
                                <select class="form-control" id="cboDeviceAuthorityQ@(itemName)" name="cboDeviceAuthorityQ@(itemName)"></select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-12 col-form-label">狀態</label>
                            <div class="col-lg-3 col-md-4 col-6">
                                <label class="control control-solid control-solid-info control--checkbox">
                                    啟用
                                    <input type="checkbox" id="cbxStatusQ@(itemName)A" name="cbxStatusQ@(itemName)" value="A" checked />
                                    <span class="control__indicator"></span>
                                </label>
                            </div>
                            <div class="col-lg-3 col-md-4 col-6">
                                <label class="control control-solid control-solid-info control--checkbox">
                                    停用
                                    <input type="checkbox" id="cbxStatusQ@(itemName)S" name="cbxStatusQ@(itemName)" value="S" />
                                    <span class="control__indicator"></span>
                                </label>
                            </div>
                        </div>
                    </fieldset>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary" onclick="Query@(itemName)()" style="margin: auto;"><i class="fal fa-wrench"></i>套用</button>
            </div>
        </div>
    </div>
</div>

<div class="row" id="listData@(itemName)">
    <div class="col-12">
        <div class="card card-shadow mb-4">
            <div class="card-body">
                <div class="row mb-3">
                    <div class="col-6 col-sm-6 col-md-6">
                        <a class="btn btn-primary" data-backdrop="static" data-keyboard="false" data-toggle="modal" data-target="#modalQuery@(itemName)" href="javascript:void(0);">
                            <i class="fal fa-search"></i>搜尋
                        </a>
                    </div>
                    <div class="col-6 col-sm-6 col-md-6 text-right">
                        <a class="btn btn-success auth-sync" href="javascript:PopSoSync@(itemName)();"><i class="fas fa-sync"></i>ERP資料同步</a>
                        <a class="btn btn-info auth-add" href="javascript:Edit@(itemName)();"><i class="fas fa-plus"></i>新增</a>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                        <div class="table-custom">
                            <table class="table table-bordered table-striped table-sticky" id="tblData@(itemName)">
                                <thead>
                                    <tr>
                                        <th scope="col" style="min-width: 75px;">#</th>
                                        <th scope="col" style="min-width: 150px;">製令編號</th>
                                        <th scope="col" style="min-width: 100px;">變更版次</th>
                                        <th scope="col" style="min-width: 100px;">變更日期</th>
                                        <th scope="col" style="min-width: 200px;">產品品號</th>
                                        <th scope="col" style="min-width: 200px;">產品品名</th>
                                        <th scope="col" style="min-width: 200px;">產品規格</th>
                                        <th scope="col" style="min-width: 100px;">拋轉狀態</th>
                                        <th class="text-center" scope="col" style="min-width: 75px;">操作</th>
                                        <th style="padding: 0; min-width: 425px;">
                                            <table class="table table-bordered table-sm mb-0 table-non-rwd table-striped-custom table-row-detail">
                                                <thead>
                                                    <tr>
                                                        <th style="width: 200px;">原材料品號</th>
                                                        <th style="width: 200px;">新材料品號</th>
                                                        <th style="width: 100px;">需領用量</th>
                                                        <th style="width: 125px;">操作</th>
                                                    </tr>
                                                </thead>
                                            </table>
                                        </th>
                                    </tr>
                                </thead>
                                <tbody></tbody>
                            </table>
                        </div>
                        <div class="pagination" id="page@(itemName)"></div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="row" id="editData@(itemName)" style="display: none;">
    <div class="col-12">
        <div class="card card-shadow mb-4">
            <div class="card-body">
                <form class="container custom-form-container" autocomplete="off" id="editForm@(itemName)">
                    <fieldset>
                        <input type="hidden" id="DeviceId" name="DeviceId" value="-1" />
                        <div class="row">
                            <div class="col-lg-12 col-md-12">
                                <div class="form-group row">
                                    <label class="col-12 col-md-3 col-lg-2 col-form-label" for="txtWoId@(itemName)">綁定ERP製令<span class="text-danger">*</span></label>
                                    <div class="col-12 col-md-9 col-lg-10">
                                        <input type="hidden" id="txtWoId@(itemName)" />
                                        <input type="hidden" id="WoErpFullNo" value="" />
                                        <div class="btn-group">
                                            <a class="btn btn-success pop-view" href="javascript:void(0);"><i class="fal fa-search"></i></a>
                                            <a class="btn btn-outline-danger pop-clear" href="javascript:void(0);" onclick="WipClear@(itemName)()"><i class="fas fa-eraser"></i></a>
                                        </div>
                                        <dl class="help-block">
                                            <dt>製令</dt>
                                            <dd id="desWoErpFullNo@(itemName)"></dd>
                                            <dt>開單日期</dt>
                                            <dd id="desMtlItemSpec@(itemName)"></dd>
                                            <dt>製令性質</dt>
                                            <dd id="desMtlItemSpec@(itemName)"></dd>
                                            <dt>品號</dt>
                                            <dd id="desMtlItemNo@(itemName)"></dd>
                                            <dt>品名</dt>
                                            <dd id="desMtlItemName@(itemName)"></dd>
                                            <dt>規格</dt>
                                            <dd id="desMtlItemSpec@(itemName)"></dd>
                                            <dt>產品庫存單位</dt>
                                            <dd id="desMtlItemSpec@(itemName)"></dd>
                                            <dt>已領套數</dt>
                                            <dd id="desMtlItemSpec@(itemName)"></dd>
                                            <dt>已生產套數</dt>
                                            <dd id="desMtlItemSpec@(itemName)"></dd>
                                            <dt>報廢數量</dt>
                                            <dd id="desMtlItemSpec@(itemName)"></dd>
                                        </dl>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12">
                                <div class="form-group row">
                                    <label class="col-12 col-md-3 col-lg-2 col-form-label" for="txtModifyDate@(itemName)">變更日期<span class="text-danger">*</span></label>
                                    <div class="col-12 col-md-9 col-lg-4">
                                        <input type="text" class="form-control" id="txtModifyDate@(itemName)" name="txtModifyDate@(itemName)" onkeypress="BandeInput(event)" onkeydown="BandCtrlV(event)"
                                               data-msg-extraRegex="請輸入正確的日期格式" data-rule-extraRegex="([12]\d{3}\-(0[1-9]|1[0-2])\-(0[1-9]|[12]\d|3[01]))" data-rule-required="true" />
                                    </div>
                                    <label class="col-12 col-md-3 col-lg-2 col-form-label" for="txtModifyVersion@(itemName)">變更版次</label>
                                    <div class="col-12 col-md-9 col-lg-4">
                                        <input type="text" class="form-control" id="txtModifyVersion@(itemName)" name="txtModifyVersion@(itemName)" placeholder="請輸入變更版次" />
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12">
                                <div class="form-group row">
                                    <label class="col-12 col-md-3 col-lg-2 col-form-label" for="cboNewWoStatus@(itemName)">製令狀態</label>
                                    <div class="col-12 col-md-9 col-lg-4">
                                        <select class="form-control" id="cboNewWoStatus@(itemName)" name="cboNewWoStatus@(itemName)"></select>
                                    </div>
                                    <label class="col-12 col-md-3 col-lg-2 col-form-label" for="cboNewSoDetailId@(itemName)">訂單資料</label>
                                    <div class="col-12 col-md-9 col-lg-4">
                                        <select class="form-control" id="cboNewSoDetailId@(itemName)" name="cboNewSoDetailId@(itemName)"></select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12">
                                <div class="form-group row">
                                    <label class="col-12 col-md-3 col-lg-2 col-form-label" for="cboNewProject@(itemName)">專案代號</label>
                                    <div class="col-12 col-md-9 col-lg-4">
                                        <select class="form-control" id="cboNewProject@(itemName)" name="cboNewProject@(itemName)"></select>
                                    </div>
                                    <label class="col-12 col-md-3 col-lg-2 col-form-label" for="cboNewPlanLotNo@(itemName)">計畫批號</label>
                                    <div class="col-12 col-md-9 col-lg-4">
                                        <select class="form-control" id="cboNewPlanLotNo@(itemName)" name="cboNewPlanLotNo@(itemName)"></select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12">
                                <div class="form-group row">
                                    <label class="col-12 col-md-3 col-lg-2 col-form-label" for="txtModifyCause@(itemName)">變更原因</label>
                                    <div class="col-12 col-md-9 col-lg-10">
                                        <textarea class="form-control" id="txtModifyCause@(itemName)" name="txtModifyCause@(itemName)"
                                                  data-msg-maxlength="必須少於 255 個字元" maxlength="255" rows="2" placeholder="請輸入變更原因"></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-12 col-md-12">
                                <div class="form-group row">
                                    <label class="col-12 col-md-3 col-lg-2 col-form-label" for="txtNewWoRemark@(itemName)">備註</label>
                                    <div class="col-12 col-md-9 col-lg-10">
                                        <textarea class="form-control" id="txtNewWoRemark@(itemName)" name="txtNewWoRemark@(itemName)"
                                                  data-msg-maxlength="必須少於 255 個字元" maxlength="255" rows="2" placeholder="請輸入備註"></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <a class="btn btn-collapse" data-toggle="collapse" href="#basic@(itemName)">
                            <span><i class="fas fa-clipboard"></i>時程/產量</span>
                            <i class="fas fa-chevron-down"></i>
                        </a>
                        <div class="collapse mb-3" id="basic@(itemName)">
                            <div class="card card-body">
                                <div class="row">
                                    <div class="col-lg-6 col-md-12 ">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="txtNewPlanQty@(itemName)">預計產量</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <input type="text" class="form-control" id="txtNewPlanQty@(itemName)" name="txtNewPlanQty@(itemName)" placeholder="請輸入預計產量" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="txtNewBomDate@(itemName)">BOM日期</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <input type="text" class="form-control" id="txtNewBomDate@(itemName)" name="txtNewBomDate@(itemName)" placeholder="請輸入BOM日期"
                                                       data-msg-extraRegex="請輸入正確的日期格式" data-rule-extraRegex="([12]\d{3}\-(0[1-9]|1[0-2])\-(0[1-9]|[12]\d|3[01]))" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="txtNewBomVersion@(itemName)">BOM版次</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <input type="text" class="form-control" id="txtNewBomVersion@(itemName)" name="txtNewBomVersion@(itemName)" placeholder="請輸入BOM版次" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="txtNewExpectedStart@(itemName)">預計開工</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <input type="text" class="form-control" id="txtNewExpectedStart@(itemName)" name="txtNewExpectedStart@(itemName)" placeholder="預計開工"
                                                       data-msg-extraRegex="請輸入正確的日期格式" data-rule-extraRegex="([12]\d{3}\-(0[1-9]|1[0-2])\-(0[1-9]|[12]\d|3[01]))" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="txtNewExpectedEnd@(itemName)">預計完工</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <input type="text" class="form-control" id="txtNewExpectedEnd@(itemName)" name="txtNewExpectedEnd@(itemName)" placeholder="預計完工"
                                                       data-msg-extraRegex="請輸入正確的日期格式" data-rule-extraRegex="([12]\d{3}\-(0[1-9]|1[0-2])\-(0[1-9]|[12]\d|3[01]))" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="txtConfirmUser@(itemName)">確認者</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <input type="text" class="form-control" id="txtConfirmUser@(itemName)" name="txtConfirmUser@(itemName)" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="txtConfirmDate@(itemName)">確認日期</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <input type="text" class="form-control" id="txtConfirmDate@(itemName)" name="txtConfirmDate@(itemName)"
                                                       data-msg-extraRegex="請輸入正確的日期格式" data-rule-extraRegex="([12]\d{3}\-(0[1-9]|1[0-2])\-(0[1-9]|[12]\d|3[01]))" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <a class="btn btn-collapse" data-toggle="collapse" href="#basic@(itemName)">
                            <span><i class="fas fa-clipboard"></i>廠內/托外</span>
                            <i class="fas fa-chevron-down"></i>
                        </a>
                        <div class="collapse mb-3" id="basic@(itemName)">
                            <div class="card card-body">
                                <div class="row">
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="cboNewFactory@(itemName)">廠別</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <select class="form-control" id="cboNewFactory@(itemName)" name="cboNewFactory@(itemName)"></select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="cboNewUserId@(itemName)">生管/採購</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <select class="form-control" id="cboNewUserId@(itemName)" name="cboNewUserId@(itemName)"></select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="cboNewInventoryId@(itemName)">入庫庫別</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <select class="form-control" id="cboNewInventoryId@(itemName)" name="cboNewInventoryId@(itemName)"></select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="cboNewUomId@(itemName)">加工單位</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <select class="form-control" id="cboNewUomId@(itemName)" name="cboNewUomId@(itemName)"></select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="cboNewProcessor@(itemName)">加工廠商</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <select class="form-control" id="cboNewProcessor@(itemName)" name="cboNewProcessor@(itemName)"></select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="cboNewProductionLine@(itemName)">生產線別</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <select class="form-control" id="cboNewProductionLine@(itemName)" name="cboNewProductionLine@(itemName)"></select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="cboNewTaxCode@(itemName)">稅別碼</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <select class="form-control" id="cboNewTaxCode@(itemName)" name="cboNewTaxCode@(itemName)"></select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="cboNewCurrencyCode@(itemName)">幣別匯率<span class="text-danger">*</span></label>
                                            <div class="col-12 col-md-9 col-lg-9" style="display: flex;">
                                                <select class="form-control" id="cboNewCurrencyCode@(itemName)" name="cboNewCurrencyCode@(itemName)" style="width:105px;" data-rule-required="true" data-msg-required="請選擇幣別"></select>
                                                <input type="number" class="form-control" id="txtNewExchange@(itemName)" name="txtNewExchange@(itemName)" placeholder="請輸入匯率" style="height: unset;width: calc(100% - 105px);" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="cboNewTaxType@(itemName)">課稅別</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <select class="form-control" id="cboNewTaxType@(itemName)" name="cboNewTaxType@(itemName)"></select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="cboNewPricingTerm@(itemName)">價格條件</label>
                                            <div class="col-12 col-md-9 col-lg-9">
                                                <select class="form-control" id="cboNewPricingTerm@(itemName)" name="cboNewPricingTerm@(itemName)"></select>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-lg-6 col-md-12">
                                        <div class="form-group row">
                                            <label class="col-12 col-md-3 col-lg-3 col-form-label" for="txtNewTaxRate@(itemName)">營業稅率</label>
