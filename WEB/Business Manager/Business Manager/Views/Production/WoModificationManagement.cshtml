@Html.Resource("js",
    @<script>
        let objPage@(itemName) = {
            "pageIndex": 0,
            "pageSize": 10
        };
        let objForm@(itemName) = "#editForm@(itemName)";
        let viewWomId = -1
        let userId@(itemName);
        let companyId@(itemName);
        let userNo@(itemName);

        $(document).ready(function () {
            GetUserInfo@(itemName)()

            ComboBoxs.Default("#cboDeviceTypeQ@(itemName)", "@Url.Action("GetType", "BasicInformation")", { "TypeSchema": "Device.DeviceType" }, "", "NA", "", "TypeName", "TypeNo");
            ComboBoxs.Default("#cboDeviceAuthorityQ@(itemName)", "@Url.Action("GetType", "BasicInformation")", { "TypeSchema": "Device.DeviceAuthority" }, "", "NA", "", "TypeName", "TypeNo");

            Query@(itemName)();


            if (!isMobile) {
                $("#cboDeviceTypeQ@(itemName), #cboDeviceAuthorityQ@(itemName)").select2({
                    width: "100%"
                });
            }
        });

        function Query@(itemName)() {
            objPage@(itemName).pageIndex = 0;
            InitData@(itemName)(objPage@(itemName));

            $("#modalQuery@(itemName)").modal("hide");
        }

        function InitData@(itemName)(objPage) {
            let innerHtml = '';
            let pageCount = 0;

            let statusList = [];
            $.each($("input[name='cbxStatusQ@(itemName)']:checked"), function () {
                statusList.push($(this).val());
            });

            let targetUrl = "@Url.Action("GetDevice", "MesBasicInformation")";
            let postData = {
                "OrderBy": "",
                "PageIndex": (objPage.pageIndex + 1),
                "PageSize": objPage.pageSize,
                "DeviceType": $("#cboDeviceTypeQ@(itemName)").val(),
                "DeviceName": $("#txtDeviceNameQ@(itemName)").val(),
                "DeviceIdentifierCode": $("#txtDeviceIdentifierCodeQ@(itemName)").val().trim(),
                "DeviceAuthority": $("#cboDeviceAuthorityQ@(itemName)").val(),
                "Status": statusList.join(",")
            };

            let data = DataAccess.Get(targetUrl, postData, false);

            if (data.status == "success") {
                pageCount = data.result.length > 0 ? parseInt(data.result[0]["TotalCount"]) : 0;

                if (pageCount > 0) {
                    objPage = PageCaculate(pageCount, objPage);

                    $.each(data.result, function (i, item) {
                        let _row = (objPage.pageSize * objPage.pageIndex + (i + 1)) + ".";

                        innerHtml += `<tr>
                                        <td>${_row}</td>
                                        <td class="ellipsis" data-toggle="tooltip" title="${item.DeviceName}" style="max-width: 150px;">${item.DeviceName}</td>
                                        <td class="ellipsis" data-toggle="tooltip" title="${item.DeviceDesc}" style="max-width: 100px;">${item.DeviceDesc}</td>
                                        <td class="ellipsis" data-toggle="tooltip" title="${item.DeviceName}" style="max-width: 100px;">${item.DeviceName}</td>
                                        <td class="ellipsis" data-toggle="tooltip" title="${item.DeviceName}" style="max-width: 200px;">${item.DeviceName}</td>
                                        <td class="ellipsis" data-toggle="tooltip" title="${item.DeviceName}" style="max-width: 200px;">${item.DeviceName}</td>
                                        <td class="ellipsis" data-toggle="tooltip" title="${item.DeviceName}" style="max-width: 200px;">${item.DeviceName}</td>
                                        <td class="ellipsis" data-toggle="tooltip" title="${item.DeviceName}" style="max-width: 100px;">${item.DeviceName}</td>
                                        <td class="text-center active-content">
                                          <div class="btn-group mb-4 show" style="margin-bottom: 0 !important; position: static;">
                                            <button type="button" class="btn btn-primary dropdown-toggle" data-tsrn-id="${item.DeviceId}" style="padding-top: 0; padding-bottom: 0;">動作</button>
                                            <div class="dropdown-menu btn-text-left-block" data-tsrn-id="${item.DeviceId}" style="overflow-y: auto; z-index: 999; max-height: 150px;">
                                              ${!!item.DeviceId ? `<a class="dropdown-item btn-text-left auth-update" href="javascript:Edit@(itemName)(${item.TsrnId});" ><i class="fas fa-eye"></i>查看</a>` : ``}
                                              ${!!item.DeviceId ? `<a class="dropdown-item btn-text-left auth-update" href="javascript:Edit@(itemName)(${item.TsrnId});" ><i class="fas fa-edit"></i>修改</a>` : ``}
                                              @*${!!item.DeviceId ? `<a class="dropdown-item btn-text-left auth-import" href="javascript:OrderAction@(itemName)(${'Import',item.TsrnId});" ><i class="fas fa-upload"></i><span>拋轉</span></a>` : item.TransferStatus == `Y` && item.ConfirmStatus == `N` ? `<a class="dropdown-item btn-text-left auth-import" href="javascript:Revise@(itemName)(${item.TsrnId});" ><i class="fas fa-reply-all"></i><span>回歸</span></a>` : ``}
                                              ${!!item.DeviceId ? `<a class="dropdown-item btn-text-left auth-import" href="javascript:OrderAction@(itemName)(${'Revise',item.TsrnId});" ><i class="fas fa-upload"></i><span>拋轉</span></a>` : item.TransferStatus == `Y` && item.ConfirmStatus == `N` ? `<a class="dropdown-item btn-text-left auth-import" href="javascript:Revise@(itemName)(${item.TsrnId});" ><i class="fas fa-reply-all"></i><span>回歸</span></a>` : ``}*@
                                              ${!!item.DeviceId ? `<a class="dropdown-item btn-text-left auth-delete" href="javascript:OrderAction@(itemName)('Delete', ${item.TsrnId});" ><i class="fas fa-trash-alt"></i>刪除</a>` : ``}
                                              ${!!item.DeviceId ? `<a class="dropdown-item btn-text-left auth-import" href="javascript:OrderAction@(itemName)('Import', ${item.DeviceId});" ><i class="fas fa-upload"></i>拋轉</a>` : ``}
                                              ${!!item.DeviceId ? `<a class="dropdown-item btn-text-left auth-import" href="javascript:OrderAction@(itemName)('Revise', ${item.DeviceId});" ><i class="fas fa-reply-all"></i>回歸</a>` : ``}
                                              ${!!item.DeviceId ? `<a class="dropdown-item btn-text-left auth-confirm" href="javascript:OrderAction@(itemName)('Confirm', ${item.DeviceId});" ><i class="fas fa-check-circle"></i>確認</a>` : ``}
                                              ${!!item.DeviceId ? `<a class="dropdown-item btn-text-left auth-reconfirm" href="javascript:OrderAction@(itemName)('ReConfirm', ${item.DeviceId});" ><i class="fas fa-undo-alt"></i>反確認</a>` : ``}
                                              ${!!item.DeviceId ? `<a class="dropdown-item btn-text-left auth-void" href="javascript:OrderAction@(itemName)('Void', ${item.DeviceId});" ><i class="fas fa-ban"></i>作廢</a>` : ``}
                                              ${!!item.DeviceId ? `<a class="dropdown-item btn-text-left auth-void" href="javascript:OrderAction@(itemName)('Void', ${item.DeviceId});" ><i class="fas fa-ban"></i>原製令比較</a>` : ``}
                                            </div>
                                          </div>
                                        </td>
                                        <td style="padding: 0;">
                                          <table class="table table-bordered table-sm mb-0 table-non-rwd table-striped-custom table-row-detail">
                                            <tbody>
                                              ${DetailData@(itemName)(item.DeviceName)}
                                            </tbody>
                                          </table>
                                        </td>
                                      </tr>`;
                    });
                } else {
                    innerHtml += `<tr>
                                    <td class="text-center" colspan="8" style="background-color: #fff3cd;">
                                      <i class="fal fa-exclamation-circle"></i>&nbsp;目前尚無資料。
                                    </td>
                                  </tr>`;
                }
            } else {
                alert(data.msg, "alert", 0);
            }

            $("#tblData@(itemName) tbody").html(innerHtml);
            TableComponentInit();
            Pager("#page@(itemName)", pageCount, objPage, InitData@(itemName));

            $("input[name='chkStatus@(itemName)']").bootstrapToggle({
                on: "啟用",
                off: "停用",
                onstyle: "success",
                offstyle: "danger",
                size: "mini"
            });


            $(".dropdown-toggle").off("click").on("click", function () {
                let tsrnId = $(this).data("tsrn-id");

                if ($(`.dropdown-menu[data-tsrn-id='${tsrnId}']`).hasClass("show")) {
                    $(".dropdown-menu").removeClass("show");
                } else {
                    $(".dropdown-menu").removeClass("show");

                    $(`.dropdown-menu[data-tsrn-id='${tsrnId}']`).addClass("show");
                    $(`.dropdown-menu[data-tsrn-id='${tsrnId}']`).css({
                        top: `${$(this).position().top + $(this).height() + 5}px`,
                        left: `${$(this).position().left - $(this).width() + 38}px`
                    });
                }
            });

            $("body").off("click").on("click", function (e) {
                if ($(".dropdown-menu").hasClass("show")) {
                    if (!$(e.target).hasClass("dropdown-toggle")
                        && !$(e.target).parents().hasClass("dropdown-toggle")) {
                        $(".dropdown-menu").removeClass("show");
                    }
                }
            });
        }

        function DetailData@(itemName)(DeviceName, source){
            let html = '';

            html += `<tr>
                        <td style="width: 200px;">${DeviceName}</td>
                        <td style="width: 200px;">${DeviceName}</td>
                        <td style="width: 100px;">${DeviceName}</td>
                        <td style="width: 125px;">${DeviceName}</td>
                    </tr>`;
            if (!!source) {
                let json = JSON.parse(source);

                for (let x = 0; x < json.data.length; x++) {
                    var TypeName = "";
                    switch (json.data[x].ProductType) {
                        case "1":
                            TypeName = "贈品量"
                            break
                        case "2":
                            TypeName = "備品量"
                            break
                    }
                    html += `<tr>
                               <td style="width: 100px; max-width: 100px;">${DeviceName}</td>
                               <td style="width: 100px; max-width: 100px;">${DeviceName}</td>
                               <td style="width: 100px; max-width: 100px;">${DeviceName}</td>
                               <td style="width: 175px; max-width: 175px;" class="ellipsis" data-toggle="tooltip" title="${json.data[x].MtlItemNo}">${json.data[x].MtlItemNo}</td>
                             </tr>`;
                }
            }

            return html;
        }

        function Edit@(itemName)(WomId) {
            viewWomId = WomId ? WomId : -1;

            Switch("#editData@(itemName), .unit-block", "#listData@(itemName)");
            //製令狀態
            ComboBoxs.Default("#cboNewWoStatus@(itemName)", "@Url.Action("GetErpData", "BasicInformation")", { "ColumnName": "Project" }, "", "NA", "", "ProjectName", "Project");
            //訂單選單
            //ComboBoxs.Default("#cboNewSoDetailId@(itemName)", "@Url.Action("GetSaleOrderSimple", "SaleOrder")", { "MtlItemId": $("#txtMtlItemId@(itemName)").val()}, "", "NA", "", "SoErpFullNo", "SoDetailId"); //訂單單位
            //專案代號
            ComboBoxs.Default("#cboNewProject@(itemName)", "@Url.Action("GetErpData", "BasicInformation")", { "ColumnName": "Project" }, "", "NA", "", "ProjectName", "Project");
            //計畫批號
            ComboBoxs.Default("#cboNewPlanLotNo@(itemName)", "@Url.Action("GetErpData", "BasicInformation")", { "ColumnName": "Project" }, "", "NA", "", "ProjectName", "Project");
            //廠別
            ComboBoxs.WithText("#cboNewFactory@(itemName)", "@Url.Action("GetErpData", "BasicInformation")", { "ColumnName": "Factory" }, "", "CHOOSE", "", "FactoryName", "Factory");
            //生管/採購
            ComboBoxs.Default("#cboNewUserId@(itemName)", "@Url.Action("GetUser", "BasicInformation")", {"CompanyId": companyId@(itemName)}, "", "NA", "", "UserWithNo", "UserId"); //確認者
            //入庫庫別
            ComboBoxs.Default("#cboNewInventoryId@(itemName)", "@Url.Action("GetInventory", "ScmBasicInformation")", {}, "", "NA", "", "InventoryWithNo", "InventoryId"); //入庫庫別
            //加工單位
            ComboBoxs.Default("#cboNewUomId@(itemName)", "@Url.Action("GetUnitOfMeasure", "PdmBasicInformation")", {"Status":"A"}, "", "NA", "", "UomNo", "UomId"); //加工單位
            //加工廠商
            ComboBoxs.WithText("#cboNewProcessor@(itemName)", "@Url.Action("GetErpData", "BasicInformation")", { "ColumnName": "Processor" }, "", "NA", "", "ProcessorName", "ProcessorNo");
            //生產線別
            ComboBoxs.WithText("#cboNewProductionLine@(itemName)", "@Url.Action("GetErpData", "BasicInformation")", { "ColumnName": "ProduceLineNo" }, "", "NA", "", "ProduceLineName", "ProduceLineNo");
            //稅別碼選單
            ComboBoxs.WithText("#cboNewTaxCode@(itemName)", "@Url.Action("GetErpData", "BasicInformation")", { "ColumnName": "TaxNo" }, "", "NA", "", "TaxName", "TaxNo");
            //課稅別選單
            ComboBoxs.Default("#cboNewTaxType@(itemName)", "@Url.Action("GetType", "BasicInformation")", { "TypeSchema": "Customer.Taxation" }, "", "NA", "", "TypeName", "TypeNo");
            //幣別選單
            ComboBoxs.WithText("#cboNewCurrencyCode@(itemName)", "@Url.Action("GetErpData", "BasicInformation")", { "ColumnName": "Currency" }, "", "NA", "", "CurrencyName", "Currency");
            //價格條件
            ComboBoxs.Default("#cboNewPricingTerm@(itemName)", "@Url.Action("GetErpData", "BasicInformation")", { "ColumnName": "PaymentTerm" }, "", "NA", "", "PaymentTermName", "PaymentTermNo");
            //付款條件選單
            ComboBoxs.Default("#cboNewPaymentTerm@(itemName)", "@Url.Action("GetErpData", "BasicInformation")", { "ColumnName": "PaymentTerm" }, "", "NA", "", "PaymentTermName", "PaymentTermNo");

            //開單日預設今日
            let currentDate = new Date();
            Suites.DatePicker("txtModifyDate@(itemName)", currentDate);
            //Suites.DatePicker("txtNewExpectedStart@(itemName), txtNewExpectedEnd@(itemName)", "");

            if (viewWomId > 0) {
                let targetUrl = "@Url.Action("GetWoModification", "Production")";
                let postData = {
                    "WomId": viewWomId
                };

                let data = DataAccess.Get(targetUrl, postData, false);

                if (data.status == "success") {
                    $.each(data.result, function (i, item) {
                        $("#cboDeviceType@(itemName)").val(item.DeviceType);
                        $("#txtDeviceName@(itemName)").val(item.DeviceName);
                        $("#txtDeviceDesc@(itemName)").val(item.DeviceDesc);
                        $("#txtDeviceIdentifierCode@(itemName)").val(item.DeviceIdentifierCode);
                        $("#cboDeviceAuthority@(itemName)").val(item.DeviceAuthority);
                    });

                    //ExpandDeviceMachine();
                } else {
                    alert(data.msg, "alert", 0);
                }
            }

            let popConfig = {
                targetSelector: "#txtWoId@(itemName)",
                popFunction: "PopViewWipOrder",
                objectWoId: "#txtWoId@(itemName)",
                objectWoErpFullNo: "#desWoErpFullNo@(itemName)",
                objectMtlItemNo: "#desMtlItemNo@(itemName)",
                objectMtlItemName: "#desMtlItemName@(itemName)",
                objectMtlItemSpec: "#desMtlItemSpec@(itemName)",
                objectPlanQty: "#desPlanQty@(itemName)"
            };
            InitPopView(popConfig);

            if (!isMobile) {
                $(`#cboNewWoStatus@(itemName), #cboNewSoDetailId@(itemName), #cboNewProject@(itemName), #cboNewPlanLotNo@(itemName)
                    , #cboNewFactory@(itemName), #cboNewUserId@(itemName), #cboNewInventoryId@(itemName), #cboNewUomId@(itemName)
                    , #cboNewProcessor@(itemName), #cboNewProductionLine@(itemName), #cboNewTaxCode@(itemName), #cboNewTaxType@(itemName)
                    , #cboNewCurrencyCode@(itemName), #cboNewPricingTerm@(itemName), #cboNewPaymentTerm@(itemName)
                    `).select2({
                    width: "100%"
                });
            }

        }

        function Save@(itemName)() {
            if ($(objForm@(itemName)).valid()) {

                if (viewWomId <= 0) {
                    let targetUrl = "@Url.Action("AddDevice", "Production")";
                    let postData = {
                        "DeviceType": $("#cboDeviceType@(itemName)").val(),
                        "DeviceName": $("#txtDeviceName@(itemName)").val(),
                        "DeviceDesc": $("#txtDeviceDesc@(itemName)").val(),
                        "DeviceIdentifierCode": $("#txtDeviceIdentifierCode@(itemName)").val(),
                        "DeviceAuthority": $("#cboDeviceAuthority@(itemName)").val(),
                    };

                    let data = DataAccess.EditContinued(targetUrl, postData, false);

                    if (data) {
                        $.each(data.result, function (i, item) {
                            $("#DeviceId").val(item.DeviceId);
                        });
                        //ExpandDeviceMachine();
                    }
                } else {
                    let targetUrl = "@Url.Action("UpdateDevice", "Production")";
                    let postData = {
                        "DeviceId": DeviceId,
                        "DeviceType": $("#cboDeviceType@(itemName)").val(),
                        "DeviceName": $("#txtDeviceName@(itemName)").val(),
                        "DeviceDesc": $("#txtDeviceDesc@(itemName)").val(),
                        "DeviceIdentifierCode": $("#txtDeviceIdentifierCode@(itemName)").val(),
                        "DeviceAuthority": $("#cboDeviceAuthority@(itemName)").val(),
                    };

                    let result = DataAccess.Update(targetUrl, postData, false, true);

                    if (result) {
                        Return@(itemName)();
                    }
                }
            }
        }

        function Delete@(itemName)(WomId) {
            swal.fire({
                titleText: "確認要刪除嗎?",
                text: "此動作無法復原，請確認是否要執行!",
                icon: "warning",
                allowOutsideClick: false,
                allowEscapeKey: false,
                showCancelButton: true,
                confirmButtonColor: '#d33',
                reverseButtons: true
            }).then((result) => {
                if (result.isConfirmed) {
                    let targetUrl = "@Url.Action("DeleteWoModification", "Production")";
                    let postData = {
                        "DeviceId": DeviceId,
                    };

                    let result = DataAccess.Delete(targetUrl, postData, false, true);

                    if (result) {
                        Query@(itemName)();
                    }
                }
            });
        }

        function OrderAction@(itemName)(Type, WomId) {
            let inConfrim = "N";
            let targetUrl = ""
            let postData = ""
            let titleText = ""
            let text = ""
            let Order = `ERP製令變更單`

            if (WomId == -1) {
                WomId = viewWomId
                inConfrim = "Y"
            }
            postData = {
                "WomId": WomId
            };
            switch (Type) {
                case "Delete":
                    titleText = `${Order}是否要進行刪除嗎?`
                    text = "執行此動作單據會無法復原，請確認是否要執行!"
                    targetUrl = "@Url.Action("DeleteWoModification", "Production")";
                    break;
                case "Import":
                    titleText = `${Order}是否要拋轉ERP嗎?`
                    text = "執行此動作單據會正式成立，請確認是否要執行!"
                    targetUrl = "@Url.Action("UpdateWomToERP", "Production")";
                    break;
                case "Revise":
                    titleText = `${Order}是否要重新編輯嗎?`
                    text = "執行此動作單據可以再以再MES重新編輯，請確認是否要執行!"
                    targetUrl = "@Url.Action("UpdateWomReviseMES", "Production")";
                    break;
                case "Confirm":
                    titleText = `${Order}是否要核單嗎?`
                    text = "執行此動作會讓單據生效,請確認是否要執行!"
                    targetUrl = "@Url.Action("UpdateWomConfirm", "Production")";
                    break;
                case "ReConfirm":
                    titleText = `${Order}是否要反確認嗎?`
                    text = "執行此動作會讓單據失效,請確認是否要執行!"
                    targetUrl = "@Url.Action("UpdateWomReconfirm", "Production")";
                    break;
                case "Void":
                    titleText = `${Order}是否要作廢嗎?`
                    text = "執行此動作無法復原,請確認是否要執行!"
                    targetUrl = "@Url.Action("UpdateWomVoid", "Production")";
                    break;
            }

            swal.fire({
                titleText: titleText,
                text: text,
                icon: "warning",
                allowOutsideClick: false,
                allowEscapeKey: false,
                showCancelButton: true,
                confirmButtonColor: '#d33',
                reverseButtons: true
            }).then((result) => {
                    if (result.isConfirmed) {

                        let result = DataAccess.Other(targetUrl, postData, false, true);

                        if (result) {
                        Query@(itemName)();

                        if (inConfrim == "Y") {
                            Return@(itemName)()
                        }
                    }
                }
            });
        }

        function PopSoSync@(itemName)() {
            $("#modalSync@(itemName)").modal({
                backdrop: "static",
                keyboard: false,
                show: true
            });
            let currentDate = new Date();
            Suites.DatePicker("txtSyncEndDate@(itemName)", currentDate);
            currentDate.setDate(currentDate.getDate() - 7);
            Suites.DatePicker("txtSyncStartDate@(itemName)", currentDate);
        }

        function CloseSync@(itemName)() {
            ResetForm("#syncForm@(itemName)");

            $("#modalSync@(itemName)").modal("hide");

            Query@(itemName)();
        }

        function Sync@(itemName)() {
            swal.fire({
                titleText: "確認執行ERP資料同步嗎?",
                text: "此動作無法復原，請確認是否要執行!",
                icon: "warning",
                allowOutsideClick: false,
                allowEscapeKey: false,
                showCancelButton: true,
                confirmButtonColor: '#d33',
                reverseButtons: true
            }).then((result) => {
                if (result.isConfirmed) {
                    let targetUrl = "@Url.Action("UpdateWomSynchronize", "Production")";
                    let postData = {
                        "RtErpFullNo": $("#txtRtErpFullNo@(itemName)").val(),
                        "SyncStartDate": ($("#cbxSyncDate@(itemName)").is(":checked") ? $("#txtSyncStartDate@(itemName)").val() : ""),
                        "SyncEndDate": ($("#cbxSyncDate@(itemName)").is(":checked") ? $("#txtSyncEndDate@(itemName)").val() : ""),
                        "NormalSync": $("#cbxNormalSync@(itemName)").is(":checked") ? "Y" : "N",
                        "TranSync": $("#cbxTranSync@(itemName)").is(":checked") ? "Y" : "N"
                    };

                    let data = DataAccess.EditContinued(targetUrl, postData, false);

                    if (data) {
                        alert(data.result, "success", 5000);
                    }

                    CloseSync@(itemName)();
                }
            });
        }

        function GetUserInfo@(itemName)() {
            let targetUrl = "@Url.Action("GetLoginByKey", "User")";
            let postData = {
                "UserNo": $.cookie("Login"),
                "KeyText": $.cookie("LoginKey")
            };

            let data = DataAccess.Get(targetUrl, postData, false);

            if (data.status == "success") {
                userId@(itemName) = data.returnData.UserId;
                companyId@(itemName) = data.returnData.CompanyId;
                userNo@(itemName) = data.returnData.UserNo;

            } else {
                alert(data.msg, "alert", 0);
            }
        }

        function Return@(itemName)() {
            ResetForm(objForm@(itemName));
            Switch("#listData@(itemName)", "#editData@(itemName), .unit-block, .advanced-block");
            Query@(itemName)();
        }


    </script>
                )